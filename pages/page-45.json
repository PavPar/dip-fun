u0445\u0440\u0430\u043d\u0438\u043b\u0438 \u0432\u0441\u0435 \u043f\u043e\u043b\u0435\u0437\u043d\u044b\u0435 \u0441\u0432\u043e\u0439\u0441\u0442\u0432\u0430 \u0438 \u0432\u0438\u0442\u0430\u043c\u0438\u043d\u044b. \u0418\u0442\u0430\u043b\u044c\u044f\u043d\u0441\u043a\u0438\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0442 \u0443\u043b\u0443\u0447\u0448\u0430\u0435\u0442 \u043e\u0431\u043c\u0435\u043d \u0432\u0435\u0449\u0435\u0441\u0442\u0432, \u0432\u044b\u0432\u043e\u0434\u0438\u0442 \u0442\u043e\u043a\u0441\u0438\u043d\u044b \u0438\u0437 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043c\u0430, \u0430 \u0442\u0430\u043a\u0436\u0435 \u0443\u043b\u0443\u0447\u0448\u0430\u0435\u0442 \u0432\u043d\u0435\u0448\u043d\u0438\u0439 \u0432\u0438\u0434 \u043a\u043e\u0436\u0438. \u0422\u0430\u043a\u0436\u0435 \u043f\u043b\u043e\u0434\u044b \u0421\u0440\u0435\u0434\u0438\u0437\u0435\u043c\u043d\u043e\u043c\u043e\u0440\u044c\u044f \u0441\u0442\u0430\u043d\u0443\u0442 \u043f\u043e\u043b\u0435\u0437\u043d\u044b\u043c\u0438 \u043f\u0440\u0438 \u043d\u0430\u0440\u0443\u0448\u0435\u043d\u0438\u044f\u0445 \u0440\u0430\u0431\u043e\u0442\u044b \u0441\u0435\u0440\u0434\u0435\u0447\u043d\u043e-\u0441\u043e\u0441\u0443\u0434\u0438\u0441\u0442\u043e\u0439 \u0441\u0438\u0441\u0442\u0435\u043c\u044b. \u041b\u0438\u043c\u043e\u043d\u043d\u044b\u0439 \u0441\u043e\u043a \u0432 \u0438\u0445 \u0441\u043e\u0441\u0442\u0430\u0432\u0435 \u0432\u044b\u0441\u0442\u0443\u043f\u0430\u0435\u0442 \u043a\u043e\u043d\u0441\u0435\u0440\u0432\u0430\u043d\u0442\u043e\u043c, \u0430 \u0432\u044b\u0440\u0430\u0436\u0435\u043d\u043d\u044b\u0439 \u0432\u043a\u0443\u0441 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0443 \u0434\u043e\u0431\u0430\u0432\u043b\u044f\u044e\u0442 \u0441\u043e\u043b\u044c \u0438 \u043f\u0435\u0440\u0435\u0446. \u0421 \u0442\u0430\u043a\u0438\u043c\u0438 \u043e\u043b\u0438\u0432\u043a\u0430\u043c\u0438 \u0441\u0443\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e \u044d\u043a\u043e\u043d\u043e\u043c\u0438\u0442\u0441\u044f \u0432\u0440\u0435\u043c\u044f, \u0432\u0435\u0434\u044c \u043f\u043e\u043b\u0443\u0444\u0430\u0431\u0440\u0438\u043a\u0430\u0442\u044b \u043d\u0435 \u043d\u0443\u0436\u043d\u043e \u0437\u0430\u0440\u0430\u043d\u0435\u0435 \u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c \u0434\u043b\u044f \u0431\u043b\u044e\u0434\u0430: \u043d\u0430\u0440\u0435\u0437\u0430\u0442\u044c, \u0438\u0437\u0431\u0430\u0432\u043b\u044f\u0442\u044c\u0441\u044f \u043e\u0442 \u043a\u043e\u0441\u0442\u043e\u0447\u0435\u043a. \u041e\u043d\u0438 \u043e\u0442\u043b\u0438\u0447\u043d\u043e \u0441\u043e\u0447\u0435\u0442\u0430\u044e\u0442\u0441\u044f \u0441 \u0440\u044b\u0431\u043d\u044b\u043c\u0438 \u0438 \u043c\u044f\u0441\u043d\u044b\u043c\u0438 \u0434\u0435\u043b\u0438\u043a\u0430\u0442\u0435\u0441\u0430\u043c\u0438. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u043e\u043b\u0438\u0432\u043a\u0438 \u0440\u0435\u0437\u0430\u043d\u044b\u0435 ARO \u0432 \u0436\u0435\u0441\u0442\u044f\u043d\u043e\u0439 \u0431\u0430\u043d\u043a\u0435 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u0411\u0440\u0435\u043d\u0434: ARO.\u0421\u0442\u0440\u0430\u043d\u0430: \u0420\u043e\u0441\u0441\u0438\u044f.\u0421\u0442\u0440\u0430\u043d\u0430 \u0431\u0440\u0435\u043d\u0434\u0430: \u0418\u0442\u0430\u043b\u0438\u044f.\u041f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c: Metro C&C.\u0412\u0438\u0434: \u0440\u0435\u0437\u0430\u043d\u044b\u0435.\u0421\u043e\u0441\u0442\u0430\u0432: \u0432\u043e\u0434\u0430, \u043c\u0430\u0441\u043b\u0438\u043d\u044b, \u0441\u043e\u043b\u044c, \u0440\u0435\u0433\u0443\u043b\u044f\u0442\u043e\u0440 \u043a\u0438\u0441\u043b\u043e\u0442\u043d\u043e\u0441\u0442\u0438.\u041f\u0438\u0449\u0435\u0432\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c \u0432 100 \u0433 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0430: \u0436\u0438\u0440\u044b \u2014 16.4 \u0433, \u0431\u0435\u043b\u043a\u0438 \u2014 1.2 \u0433, \u0443\u0433\u043b\u0435\u0432\u043e\u0434\u044b \u2014 4.8 \u0433.\u042d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: 172 \u043a\u043a\u0430\u043b.\u0423\u0441\u043b\u043e\u0432\u0438\u044f \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f: \u041e\u0442 0 \u0434\u043e 25\u00b0C.\u0421\u0440\u043e\u043a \u0433\u043e\u0434\u043d\u043e\u0441\u0442\u0438: 3 \u0433\u043e\u0434\u0430.\u0421\u043e\u0441\u0442\u0430\u0432: \u043e\u043b\u0438\u0432\u043a\u0438, \u043a\u0430\u043f\u0435\u0440\u0441\u044b, \u0432\u043e\u0434\u0430, \u0441\u043e\u043b\u044c, \u043f\u043e\u0434\u043a\u0438\u0441\u043b\u0438\u0442\u0435\u043b\u0438 (\u043c\u043e\u043b\u043e\u0447\u043d\u0430\u044f \u043a\u0438\u0441\u043b\u043e\u0442\u0430 \u0415-270, \u043b\u0438\u043c\u043e\u043d\u043d\u0430\u044f \u043a\u0438\u0441\u043b\u043e\u0442\u0430 \u0415-330), \u0443\u0441\u0438\u043b\u0438\u0442\u0435\u043b\u044c \u0432\u043a\u0443\u0441\u0430 \u0433\u043b\u044e\u043a\u043e\u043d\u0430\u0442 \u043d\u0430\u0442\u0440\u0438\u044f \u0415-621, \u0441\u0442\u0430\u0431\u0438\u043b\u0438\u0437\u0430\u0442\u043e\u0440 \u043d\u0430\u0442\u0440\u0438\u0435\u0432\u044b\u0439 \u0430\u043b\u044c\u0433\u0438\u043d\u0430\u0442 \u0415-401, \u0430\u043d\u0442\u0438\u043e\u043a\u0441\u0438\u0434\u0430\u043d\u0442 \u0430\u0441\u043a\u043e\u0440\u0431\u0438\u043d\u043e\u0432\u0430\u044f \u043a\u0438\u0441\u043b\u043e\u0442\u0430 \u0415-300.\u0423\u043f\u0430\u043a\u043e\u0432\u043a\u0430: \u0436\u0435\u0441\u0442\u044f\u043d\u0430\u044f \u0431\u0430\u043d\u043a\u0430.","4":"\u0421\u0435\u043b\u044c\u0434\u044c \u0420\u0423\u0421\u0421\u041a\u041e\u0415 \u041c\u041e\u0420\u0415 \u0444\u0438\u043b\u0435-\u043a\u0443\u0441\u043e\u0447\u043a\u0438 \u0432 \u043c\u0430\u0441\u043b\u0435, 500 \u0433 \u2013 \u044d\u0442\u043e \u043f\u043e\u043b\u043d\u043e\u0441\u0442\u044c\u044e \u043d\u0430\u0442\u0443\u0440\u0430\u043b\u044c\u043d\u044b\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0442 \u0431\u0435\u0437 \u043a\u0440\u0430\u0441\u0438\u0442\u0435\u043b\u0435\u0439 \u0438 \u0434\u043e\u0431\u0430\u0432\u043e\u043a. \u041f\u0440\u0438 \u0438\u0437\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u0438\u0438 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u0443\u0435\u0442\u0441\u044f \u0442\u043e\u043b\u044c\u043a\u043e \u0441\u0432\u0435\u0436\u0430\u044f \u0440\u044b\u0431\u0430. \u041d\u0435\u0436\u043d\u043e\u0435 \u0444\u0438\u043b\u0435 \u0441\u0435\u043b\u044c\u0434\u0438 \u0437\u0430\u043b\u0438\u0442\u043e \u043f\u043e\u0434\u0441\u043e\u043b\u043d\u0435\u0447\u043d\u044b\u043c \u043c\u0430\u0441\u043b\u043e\u043c \u0432\u044b\u0441\u043e\u043a\u043e\u0439 \u0441\u0442\u0435\u043f\u0435\u043d\u0438 \u043e\u0447\u0438\u0441\u0442\u043a\u0438, \u0431\u0435\u0437 \u0437\u0430\u043f\u0430\u0445\u0430 \u0438 \u0445\u043e\u043b\u0435\u0441\u0442\u0435\u0440\u0438\u043d\u0430. \u041a\u043e\u043d\u0441\u0435\u0440\u0432\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0442 \u043c\u043e\u0436\u043d\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u043a\u0430\u043a \u0441\u0430\u043c\u043e\u0441\u0442\u043e\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0435 \u0431\u043b\u044e\u0434\u043e \u0441 \u0433\u0430\u0440\u043d\u0438\u0440\u043e\u043c, \u0442\u0430\u043a \u0438 \u0434\u043b\u044f \u043f\u0440\u0438\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u0438\u044f \u0431\u0443\u0442\u0435\u0440\u0431\u0440\u043e\u0434\u043e\u0432 \u0438 \u0441\u0430\u043b\u0430\u0442\u043e\u0432. \u0422\u043e\u0432\u0430\u0440 \u043f\u0440\u043e\u0438\u0437\u0432\u0435\u0434\u0435\u043d \u0441 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0445 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0439. \u041f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044f \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u043e\u0433\u043e \u0431\u0440\u0435\u043d\u0434\u0430 \u0420\u0423\u0421\u0421\u041a\u041e\u0415 \u041c\u041e\u0420\u0415 \u043d\u0435 \u043e\u0441\u0442\u0430\u0432\u0438\u0442 \u0440\u0430\u0432\u043d\u043e\u0434\u0443\u0448\u043d\u044b\u043c\u0438 \u043b\u044e\u0431\u0438\u0442\u0435\u043b\u0435\u0439 \u0440\u044b\u0431\u044b. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u0441\u0435\u043b\u044c\u0434\u044c \u0420\u0423\u0421\u0421\u041a\u041e\u0415 \u041c\u041e\u0420\u0415 \u0444\u0438\u043b\u0435-\u043a\u0443\u0441\u043e\u0447\u043a\u0438 \u0432 \u043c\u0430\u0441\u043b\u0435, 500 \u0433 \u0432\u0441\u0435\u0433\u0434\u0430 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u041f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c: \u0410\u041e \u0420\u0443\u0441\u0441\u043a\u043e\u0435 \u043c\u043e\u0440\u0435, \u0420\u043e\u0441\u0441\u0438\u044f, \u041c\u043e\u0441\u043a\u043e\u0432\u0441\u043a\u0430\u044f \u043e\u0431\u043b., \u0411\u0430\u043b\u0430\u0448\u0438\u0445\u0438\u043d\u0441\u043a\u0438\u0439 \u0440-\u043d., \u0443\u043b. \u0421\u0442\u0430\u043d\u0446\u0438\u044f \u0421\u0442\u0440\u043e\u0439\u043a\u0430, \u0432\u043b. 1, \u0441\u0442\u0440. 2.\u0421\u043e\u0441\u0442\u0430\u0432: \u0444\u0438\u043b\u0435 \u0441\u0435\u043b\u044c\u0434\u0438, \u043c\u0430\u0441\u043b\u043e \u0440\u0430\u0441\u0442\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0435, \u0441\u043e\u043b\u044c, \u0440\u0435\u0433\u0443\u043b\u044f\u0442\u043e\u0440\u044b \u043a\u0438\u0441\u043b\u043e\u0442\u043d\u043e\u0441\u0442\u0438: \u0432\u0438\u043d\u043d\u0430\u044f, \u043b\u0438\u043c\u043e\u043d\u043d\u0430\u044f \u043a\u0438\u0441\u043b\u043e\u0442\u044b; \u0443\u0441\u0438\u043b\u0438\u0442\u0435\u043b\u044c \u0432\u043a\u0443\u0441\u0430 \u0438 \u0430\u0440\u043e\u043c\u0430\u0442\u0430 \u0433\u043b\u0443\u0442\u0430\u043c\u0430\u0442 \u043d\u0430\u0442\u0440\u0438\u044f, \u043a\u043e\u043d\u0441\u0435\u0440\u0432\u0430\u043d\u0442\u044b: \u0431\u0435\u043d\u0437\u043e\u0430\u0442 \u043d\u0430\u0442\u0440\u0438\u044f, \u0441\u043e\u0440\u0431\u0430\u0442 \u043a\u0430\u043b\u0438\u044f.\u041f\u0438\u0449\u0435\u0432\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: \u0431\u0435\u043b\u043a\u0438 \u2013 13 \u0433, \u0436\u0438\u0440\u044b \u2013 29 \u0433.\u042d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: 240 \u043a\u043a\u0430\u043b.\u0421\u0440\u043e\u043a \u0433\u043e\u0434\u043d\u043e\u0441\u0442\u0438: 90 \u0441\u0443\u0442\u043e\u043a.\u0423\u0441\u043b\u043e\u0432\u0438\u044f \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f: \u0445\u0440\u0430\u043d\u0438\u0442\u044c \u043f\u0440\u0438 \u0442\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0435 \u043e\u0442 -3\u00b0C \u0434\u043e 6\u00b0C.","5":" \u0412\u0438\u043d\u043e J. P. CHENET, Colombard-Sauvignon, Vin de France, \u0431\u0435\u043b\u043e\u0435, \u0441\u0443\u0445\u043e\u0435, 0,75 \u043b  \u0443\u0432\u0438\u0434\u0435\u043b\u043e \u0441\u0432\u0435\u0442 \u0432 \u0443\u0442\u043e\u043d\u0447\u0435\u043d\u043d\u043e\u0439 \u0438 \u0438\u0437\u044b\u0441\u043a\u0430\u043d\u043d\u043e\u0439 \u0424\u0440\u0430\u043d\u0446\u0438\u0438. \u042d\u0442\u043e \u0432\u043e\u0441\u0445\u0438\u0442\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u043a\u0443\u043f\u0430\u0436, \u0438\u0437\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u043d\u044b\u0439 \u0438\u0437 \u043e\u0442\u0431\u043e\u0440\u043d\u044b\u0445 \u044f\u0433\u043e\u0434 \u0441\u043e\u0440\u0442\u043e\u0432 \u041a\u0430\u0431\u0435\u0440\u043d\u0435 \u0421\u043e\u0432\u0438\u043d\u044c\u043e\u043d \u0438 \u041a\u043e\u043b\u043e\u043c\u0431\u0430\u0440. \u041c\u0435\u0440\u043b\u043e. \u0411\u043b\u0430\u0433\u043e\u0440\u043e\u0434\u043d\u044b\u0439 \u043d\u0430\u043f\u0438\u0442\u043e\u043a \u043e\u0442\u043a\u0440\u043e\u0435\u0442\u0441\u044f \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u043c\u0443 \u0438 \u043d\u0435\u0441\u043f\u0435\u0448\u043d\u043e\u043c\u0443 \u0434\u0435\u0433\u0443\u0441\u0442\u0430\u0442\u043e\u0440\u0443 \u0442\u0440\u0435\u043c\u044f \u0441\u0432\u043e\u0438\u043c\u0438 \u043f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u044b\u043c\u0438 \u0433\u0440\u0430\u043d\u044f\u043c\u0438. \u041f\u0435\u0440\u0432\u0430\u044f \u2013 \u044d\u0441\u0442\u0435\u0442\u0438\u043a\u0430: \u0438\u0437 \u044d\u043b\u0435\u0433\u0430\u043d\u0442\u043d\u043e\u0439 \u0431\u0443\u0442\u044b\u043b\u043a\u0438 \u0431\u043e\u043a\u0430\u043b \u043d\u0430\u0447\u043d\u0435\u0442 \u043d\u0430\u043f\u043e\u043b\u043d\u044f\u0442\u044c \u0441\u0432\u0435\u0442\u043b\u043e-\u0436\u0435\u043b\u0442\u0430\u044f \u0441 \u0434\u0440\u0430\u0433\u043e\u0446\u0435\u043d\u043d\u044b\u043c\u0438 \u0437\u043e\u043b\u043e\u0442\u044b\u043c\u0438 \u0438 \u0438\u0437\u0443\u043c\u0440\u0443\u0434\u043d\u044b\u043c\u0438 \u043f\u0435\u0440\u0435\u043b\u0438\u0432\u0430\u043c\u0438 \u0436\u0438\u0434\u043a\u043e\u0441\u0442\u044c. \u0418\u043d\u0442\u0435\u043d\u0441\u0438\u0432\u043d\u044b\u0439 \u0430\u0440\u043e\u043c\u0430\u0442 \u0447\u0430\u0440\u0443\u0435\u0442 \u0441\u043e\u0447\u043d\u043e\u0441\u0442\u044c\u044e \u0438 \u0441\u043f\u0435\u043b\u043e\u0441\u0442\u044c\u044e \u0444\u0440\u0443\u043a\u0442\u043e\u0432. \u0410 \u0447\u0438\u0441\u0442\u044b\u0439 \u0438 \u0441\u0432\u0435\u0436\u0438\u0439 \u0432\u043a\u0443\u0441, \u0432\u0442\u043e\u0440\u044f\u0449\u0438\u0439 \u0430\u0440\u043e\u043c\u0430\u0442\u0443, \u0441\u043e\u0431\u043b\u0430\u0437\u043d\u044f\u0435\u0442 \u0441\u0432\u043e\u0435\u0439 \u0444\u0440\u0443\u043a\u0442\u043e\u0432\u043e\u0441\u0442\u044c\u044e \u0438 \u0438\u0434\u0435\u0430\u043b\u044c\u043d\u044b\u043c \u0431\u0430\u043b\u0430\u043d\u0441\u043e\u043c \u043a\u0438\u0441\u043b\u043e\u0442\u043d\u043e\u0441\u0442\u0438, \u043e\u0441\u0442\u0430\u0432\u043b\u044f\u044f \u043f\u0440\u0438\u044f\u0442\u043d\u043e\u0435 \u0438 \u0434\u043b\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0435 \u043f\u043e\u0441\u043b\u0435\u0432\u043a\u0443\u0441\u0438\u0435. \u0422\u0440\u0430\u0434\u0438\u0446\u0438\u043e\u043d\u043d\u043e \u044d\u0442\u043e \u0432\u0438\u043d\u043e \u043f\u0440\u0435\u043a\u0440\u0430\u0441\u043d\u043e \u0434\u043e\u043f\u043e\u043b\u043d\u0438\u0442 \u0431\u043b\u044e\u0434\u0430 \u0438\u0437 \u043d\u0435\u0436\u0438\u0440\u043d\u043e\u0439 \u0440\u044b\u0431\u044b \u0438 \u043c\u043e\u0440\u0435\u043f\u0440\u043e\u0434\u0443\u043a\u0442\u043e\u0432, \u043e\u0440\u0438\u0433\u0438\u043d\u0430\u043b\u044c\u043d\u0443\u044e \u0441\u044b\u0440\u043d\u0443\u044e \u0442\u0430\u0440\u0435\u043b\u043a\u0443 \u0441 \u043c\u044f\u0433\u043a\u0438\u043c\u0438 \u0441\u044b\u0440\u0430\u043c\u0438. \u0420\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u0435\u0442\u0441\u044f \u043f\u043e\u0434\u0430\u0432\u0430\u0442\u044c \u043d\u0430\u043f\u0438\u0442\u043e\u043a \u043a \u0441\u0442\u043e\u043b\u0443, \u043f\u0440\u0435\u0434\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u043a\u0430\u043a \u0441\u043b\u0435\u0434\u0443\u0435\u0442 \u043e\u0445\u043b\u0430\u0434\u0438\u0432 \u2013 \u0434\u043e 10-12\u0421. \u041a\u0440\u0435\u043f\u043e\u0441\u0442\u044c \u043d\u0430\u043f\u0438\u0442\u043a\u0430 \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 11%. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438  \u0432\u0438\u043d\u043e J. P. CHENET, Colombard-Sauvignon, Vin de France, \u0431\u0435\u043b\u043e\u0435, \u0441\u0443\u0445\u043e\u0435, 0,75 \u043b  \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO. \u041c\u0435\u0440\u044b \u043f\u0440\u0435\u0434\u043e\u0441\u0442\u043e\u0440\u043e\u0436\u043d\u043e\u0441\u0442\u0438: \u0430\u043b\u043a\u043e\u0433\u043e\u043b\u044c \u043f\u0440\u043e\u0442\u0438\u0432\u043e\u043f\u043e\u043a\u0430\u0437\u0430\u043d \u0434\u0435\u0442\u044f\u043c \u0438 \u043f\u043e\u0434\u0440\u043e\u0441\u0442\u043a\u0430\u043c \u0434\u043e \u0432\u043e\u0441\u0435\u043c\u043d\u0430\u0434\u0446\u0430\u0442\u0438 \u043b\u0435\u0442, \u0431\u0435\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u043c \u0438 \u043a\u043e\u0440\u043c\u044f\u0449\u0438\u043c \u0436\u0435\u043d\u0449\u0438\u043d\u0430\u043c, \u043b\u0438\u0446\u0430\u043c \u0441 \u0437\u0430\u0431\u043e\u043b\u0435\u0432\u0430\u043d\u0438\u044f\u043c\u0438 \u0446\u0435\u043d\u0442\u0440\u0430\u043b\u044c\u043d\u043e\u0439 \u043d\u0435\u0440\u0432\u043d\u043e\u0439 \u0441\u0438\u0441\u0442\u0435\u043c\u044b, \u043f\u043e\u0447\u0435\u043a, \u043f\u0435\u0447\u0435\u043d\u0438 \u0438 \u0434\u0440\u0443\u0433\u0438\u0445 \u043e\u0440\u0433\u0430\u043d\u043e\u0432 \u043f\u0438\u0449\u0435\u0432\u0430\u0440\u0435\u043d\u0438\u044f. \u0427\u0440\u0435\u0437\u043c\u0435\u0440\u043d\u043e\u0435 \u0443\u043f\u043e\u0442\u0440\u0435\u0431\u043b\u0435\u043d\u0438\u0435 \u0430\u043b\u043a\u043e\u0433\u043e\u043b\u044f \u0432\u0440\u0435\u0434\u0438\u0442 \u0432\u0430\u0448\u0435\u043c\u0443 \u0437\u0434\u043e\u0440\u043e\u0432\u044c\u044e.","6":"\u041f\u0435\u0440\u0435\u0446 Metro Chef \u0447\u0438\u043b\u0438 \u043c\u043e\u043b\u043e\u0442\u044b\u0439, 365 \u0433 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0441\u043e\u0431\u043e\u0439 \u043d\u0430\u0442\u0443\u0440\u0430\u043b\u044c\u043d\u044b\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0442 \u0432\u044b\u0441\u043e\u043a\u043e\u0433\u043e \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0430. \u041f\u0435\u0440\u0435\u0446 \u0447\u0438\u043b\u0438 \u0441\u043f\u043e\u0441\u043e\u0431\u0435\u043d \u0432\u043e\u0437\u0431\u0443\u0434\u0438\u0442\u044c \u0430\u043f\u043f\u0435\u0442\u0438\u0442, \u0430 \u0442\u0430\u043a\u0436\u0435 \u0432 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0438\u0445 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430\u0445 \u043f\u043e\u043b\u043e\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u043d\u0430 \u043f\u0438\u0449\u0435\u0432\u0430\u0440\u0435\u043d\u0438\u0438. \u0415\u0433\u043e \u043c\u043e\u0436\u043d\u043e \u0441\u043e\u0435\u0434\u0438\u043d\u044f\u0442\u044c \u0441 \u0440\u0430\u0437\u043d\u044b\u043c\u0438 \u0441\u043f\u0435\u0446\u0438\u044f\u043c\u0438: \u0441\u0440\u0435\u0434\u0438\u0437\u0435\u043c\u043d\u043e\u043c\u043e\u0440\u0441\u043a\u0438\u043c\u0438 \u0438\u043b\u0438 \u0438\u0442\u0430\u043b\u044c\u044f\u043d\u0441\u043a\u0438\u043c\u0438 \u0442\u0440\u0430\u0432\u0430\u043c\u0438, \u0434\u0443\u0448\u0438\u0441\u0442\u044b\u043c \u043f\u0435\u0440\u0446\u0435\u043c, \u0441 \u0431\u0430\u0437\u0438\u043b\u0438\u043a\u043e\u043c \u0438 \u043e\u0440\u0435\u0433\u0430\u043d\u043e. \u041f\u0435\u0440\u0435\u0446 \u0447\u0438\u043b\u0438 \u043f\u043e\u043c\u043e\u0433\u0430\u0435\u0442 \u0432 \u043f\u043e\u043b\u043d\u043e\u0439 \u043c\u0435\u0440\u0435 \u0440\u0430\u0441\u043a\u0440\u044b\u0442\u044c \u0432\u043a\u0443\u0441 \u0431\u043b\u044e\u0434\u0430 \u0438 \u043f\u043e\u0434\u0447\u0435\u0440\u043a\u043d\u0443\u0442\u044c \u0435\u0433\u043e. \u041e\u043d \u043f\u043e\u0434\u0445\u043e\u0434\u0438\u0442 \u0434\u043b\u044f \u0434\u043e\u0431\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u0432 \u043c\u044f\u0441\u043e, \u0440\u044b\u0431\u0443, \u0441\u0430\u043b\u0430\u0442\u044b, \u0432 \u043f\u0438\u0446\u0446\u0443 \u0438 \u0441\u043e\u0443\u0441\u044b. \u041a\u0440\u043e\u043c\u0435 \u0442\u043e\u0433\u043e, \u044d\u0442\u043e \u043d\u0435\u0437\u0430\u043c\u0435\u043d\u0438\u043c\u044b\u0439 \u0438\u043d\u0433\u0440\u0435\u0434\u0438\u0435\u043d\u0442 \u0434\u043b\u044f \u0441\u043e\u0437\u0434\u0430\u043d\u0438\u044f \u0431\u043b\u044e\u0434 \u0438\u043d\u0434\u0438\u0439\u0441\u043a\u043e\u0439, \u043c\u0435\u043a\u0441\u0438\u043a\u0430\u043d\u0441\u043a\u043e\u0439, \u043a\u0430\u0432\u043a\u0430\u0437\u0441\u043a\u043e\u0439 \u043a\u0443\u0445\u043e\u043d\u044c. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u043f\u0435\u0440\u0435\u0446 Metro Chef \u0447\u0438\u043b\u0438 \u043c\u043e\u043b\u043e\u0442\u044b\u0439, 365 \u0433 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u0412\u044b\u043f\u0443\u0441\u043a\u0430\u044f \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044e \u043f\u043e\u0434 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c\u0438 \u0442\u043e\u0440\u0433\u043e\u0432\u044b\u043c\u0438 \u043c\u0430\u0440\u043a\u0430\u043c\u0438 METRO, \u043c\u044b \u0441\u0442\u0440\u0435\u043c\u0438\u043c\u0441\u044f \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u0443\u0434\u043e\u0432\u043b\u0435\u0442\u0432\u043e\u0440\u0438\u0442\u044c \u043f\u043e\u0442\u0440\u0435\u0431\u043d\u043e\u0441\u0442\u0438 \u043d\u0430\u0448\u0438\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432. \u041d\u0430\u0448\u0430 \u0446\u0435\u043b\u044c \u2013 \u043f\u043e\u043d\u044f\u0442\u044c, \u043a\u0430\u043a\u043e\u0439 \u0430\u0441\u0441\u043e\u0440\u0442\u0438\u043c\u0435\u043d\u0442 \u0434\u043b\u044f \u043d\u0438\u0445 \u0430\u043a\u0442\u0443\u0430\u043b\u0435\u043d \u0438 \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0438\u0442\u044c \u0435\u0433\u043e \u043f\u043e \u043f\u0440\u0438\u0432\u043b\u0435\u043a\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u0446\u0435\u043d\u0435. \u041c\u044b \u0442\u0430\u043a\u0436\u0435 \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0438\u0437\u0443\u0447\u0430\u0435\u043c \u0442\u0435\u043d\u0434\u0435\u043d\u0446\u0438\u0438 \u0440\u044b\u043d\u043a\u0430, \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u0442\u044c \u0432 \u043a\u0443\u0440\u0441\u0435 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u0442\u0440\u0435\u043d\u0434\u043e\u0432 \u0438 \u0441\u0432\u043e\u0435\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0442\u044c \u0440\u0435\u0448\u0435\u043d\u0438\u044f \u043f\u043e\u0434 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c\u0438 \u0442\u043e\u0440\u0433\u043e\u0432\u044b\u043c\u0438 \u043c\u0430\u0440\u043a\u0430\u043c\u0438 \u0432 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0438 \u0441 \u043d\u0438\u043c\u0438. \u0411\u0440\u0435\u043d\u0434 METRO Chef \u0445\u043e\u0440\u043e\u0448\u043e \u0438\u0437\u0432\u0435\u0441\u0442\u0435\u043d \u0432\u043e \u0432\u0441\u0435\u043c \u043c\u0438\u0440\u0435. \u041f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044f \u044d\u0442\u043e\u0439 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u0439 \u043c\u0430\u0440\u043a\u0438 \u043e\u0442\u043b\u0438\u0447\u0430\u0435\u0442\u0441\u044f \u043e\u0442\u043c\u0435\u043d\u043d\u044b\u043c \u0432\u043a\u0443\u0441\u043e\u043c \u0438 \u043f\u0440\u0435\u0432\u043e\u0441\u0445\u043e\u0434\u043d\u044b\u043c \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e\u043c.","7":"\u0422\u0430\u0440\u0442 Metro Chef \u043c\u0430\u043b\u0438\u043d\u0430, 900\u0433 \u2014 \u043e\u0442\u043a\u0440\u044b\u0442\u044b\u0439 \u043f\u0438\u0440\u043e\u0433 \u0441 \u044f\u0433\u043e\u0434\u043d\u043e\u0439 \u043d\u0430\u0447\u0438\u043d\u043a\u043e\u0439, \u043a\u043b\u0430\u0441\u0441\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0431\u043b\u044e\u0434\u043e \u0444\u0440\u0430\u043d\u0446\u0443\u0437\u0441\u043a\u043e\u0439 \u043a\u0443\u0445\u043d\u0438. \u041f\u0440\u0438\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d \u043e\u043d \u0438\u0437 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u0435\u0441\u043e\u0447\u043d\u043e\u0433\u043e \u0442\u0435\u0441\u0442\u0430 \u0438 \u0434\u043e\u043f\u043e\u043b\u043d\u0435\u043d \u0441\u043f\u0435\u043b\u044b\u043c\u0438 \u044f\u0433\u043e\u0434\u0430\u043c\u0438 \u043c\u0430\u043b\u0438\u043d\u044b. \u042d\u0442\u043e \u0432\u043a\u0443\u0441\u043d\u044b\u0439 \u0438 \u0441\u044b\u0442\u043d\u044b\u0439 \u0434\u0435\u0441\u0435\u0440\u0442, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u0442\u0441\u044f \u0438 \u0434\u0435\u0442\u044f\u043c, \u0438 \u0432\u0437\u0440\u043e\u0441\u043b\u044b\u043c. \u0422\u0430\u0440\u0442 \u043e\u0431\u043b\u0430\u0434\u0430\u0435\u0442 \u0445\u0440\u0443\u0441\u0442\u044f\u0449\u0435\u0439 \u043e\u0441\u043d\u043e\u0432\u043e\u0439 \u0441 \u043d\u0435\u0431\u043e\u043b\u044c\u0448\u0438\u043c\u0438 \u0431\u043e\u0440\u0442\u0438\u043a\u0430\u043c\u0438 \u0438 \u043e\u0442\u043b\u0438\u0447\u043d\u043e \u0441\u043e\u0447\u0435\u0442\u0430\u0435\u0442\u0441\u044f \u0441 \u043a\u0430\u043a\u0430\u043e, \u043b\u0430\u0442\u0442\u0435, \u043a\u0430\u043f\u0443\u0447\u0438\u043d\u043e \u0438 \u0434\u0440\u0443\u0433\u0438\u043c\u0438 \u043a\u043e\u0444\u0435\u0439\u043d\u044b\u043c\u0438 \u043d\u0430\u043f\u0438\u0442\u043a\u0430\u043c\u0438, \u0430 \u0442\u0430\u043a\u0436\u0435 \u0447\u0435\u0440\u043d\u044b\u043c \u0438\u043b\u0438 \u0437\u0435\u043b\u0435\u043d\u044b\u043c \u0447\u0430\u0435\u043c, \u043a\u043e\u043c\u043f\u043e\u0442\u043e\u043c \u0438 \u0443\u0437\u0432\u0430\u0440\u043e\u043c. \u0417\u0430\u043c\u043e\u0440\u043e\u0436\u0435\u043d\u043d\u044b\u0439 \u043f\u0438\u0440\u043e\u0433 \u0431\u044b\u0441\u0442\u0440\u043e \u0433\u043e\u0442\u043e\u0432\u0438\u0442\u0441\u044f, \u0430 \u043f\u043e\u0434\u0430\u0442\u044c \u0435\u0433\u043e \u043c\u043e\u0436\u043d\u043e \u043a\u0430\u043a \u043a \u043f\u0440\u0430\u0437\u0434\u043d\u0438\u0447\u043d\u043e\u043c\u0443, \u0442\u0430\u043a \u0438 \u043f\u043e\u0432\u0441\u0435\u0434\u043d\u0435\u0432\u043d\u043e\u043c\u0443 \u0441\u0442\u043e\u043b\u0443.METRO CHEF \u2013 \u044d\u0442\u043e \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u0430\u044f \u0422\u041c Metro C&C, \u043f\u043e\u0434 \u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u0440\u0435\u0430\u043b\u0438\u0437\u0443\u0435\u0442\u0441\u044f \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044f, \u0441\u043e\u0437\u0434\u0430\u043d\u043d\u0430\u044f \u043f\u0440\u043e\u0444\u0435\u0441\u0441\u0438\u043e\u043d\u0430\u043b\u0430\u043c\u0438 \u0434\u043b\u044f \u0448\u0435\u0444-\u043f\u043e\u0432\u0430\u0440\u043e\u0432 \u0438 \u043b\u044e\u0434\u0435\u0439, \u0443\u0432\u043b\u0435\u0447\u0435\u043d\u043d\u044b\u0445 \u0433\u0430\u0441\u0442\u0440\u043e\u043d\u043e\u043c\u0438\u0435\u0439. \u042d\u0442\u043e \u0440\u0430\u0437\u043d\u043e\u043e\u0431\u0440\u0430\u0437\u043d\u044b\u0435 \u043e\u0440\u0435\u0445\u0438, \u043c\u043e\u043b\u043e\u0447\u043d\u044b\u0435 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u044b, \u043a\u043e\u043d\u0441\u0435\u0440\u0432\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0435 \u043e\u0432\u043e\u0449\u0438, \u0433\u0440\u0438\u0431\u044b, \u043a\u043e\u043d\u0434\u0438\u0442\u0435\u0440\u0441\u043a\u0438\u0435 \u0438\u0437\u0434\u0435\u043b\u0438\u044f \u0438 \u043c\u043d\u043e\u0433\u043e\u0435 \u0434\u0440\u0443\u0433\u043e\u0435. \u041f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044f \u0431\u0440\u0435\u043d\u0434\u0430 \u0431\u0435\u0437\u043e\u043f\u0430\u0441\u043d\u0430 \u0438 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u0435\u0442 \u0432\u0441\u0435\u043c \u0441\u0442\u0430\u043d\u0434\u0430\u0440\u0442\u0430\u043c \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0430. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u0442\u0430\u0440\u0442 Metro Chef \u043c\u0430\u043b\u0438\u043d\u0430, 900\u0433 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.","8":"\u0421\u0435\u043b\u044c\u0434\u044c \u0444\u0438\u043b\u0435 \u043e\u0442\u0431\u043e\u0440\u043d\u043e\u0435 \u041c\u0410\u0422\u0418\u0410\u0421 \u0421\u0430\u043d\u0442\u0430 \u0411\u0440\u0435\u043c\u043e\u0440 XXL, 300 \u0433 \u2013 \u044d\u0442\u043e \u043d\u0435\u0436\u043d\u0435\u0439\u0448\u0438\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0442, \u0441\u043e\u0437\u0434\u0430\u043d\u043d\u044b\u0439 \u0441\u0430\u043c\u043e\u0439 \u043f\u0440\u0438\u0440\u043e\u0434\u043e\u0439. \u0420\u044b\u0431\u0430 \u043d\u0435 \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u0442 \u0433\u043b\u0443\u0442\u0430\u043c\u0430\u0442 \u043d\u0430\u0442\u0440\u0438\u044f, \u0430 \u0442\u0430\u043a\u0436\u0435 \u0438\u043c\u0435\u0435\u0442 \u043d\u0438\u0437\u043a\u0438\u0439 \u043f\u0440\u043e\u0446\u0435\u043d\u0442 \u0441\u043e\u043b\u0438. \u041e\u0442\u0431\u043e\u0440\u043d\u043e\u0435 \u0444\u0438\u043b\u0435 \u0438\u0437\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u043e \u0438\u0437 \u043a\u0440\u0443\u043f\u043d\u044b\u0445 \u0440\u044b\u0431 \u0438 \u0443\u0434\u043e\u0431\u043d\u043e \u0434\u043b\u044f \u043f\u0440\u0438\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u0438\u044f \u0441\u0430\u043c\u044b\u0445 \u0440\u0430\u0437\u043d\u043e\u043e\u0431\u0440\u0430\u0437\u043d\u044b\u0445 \u0431\u043b\u044e\u0434. \u0421\u0435\u043b\u044c\u0434\u044c \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u0431\u043e\u0433\u0430\u0442\u044b\u043c \u0438\u0441\u0442\u043e\u0447\u043d\u0438\u043a\u043e\u043c \u0431\u0435\u043b\u043a\u0430, \u0444\u043e\u043b\u0438\u0435\u0432\u043e\u0439 \u043a\u0438\u0441\u043b\u043e\u0442\u044b, \u0430 \u0442\u0430\u043a\u0436\u0435 \u043d\u0435\u043e\u0431\u0445\u043e\u0434\u0438\u043c\u044b\u0445 \u0447\u0435\u043b\u043e\u0432\u0435\u043a\u0443 \u041e\u043c\u0435\u0433\u0430-3 \u0436\u0438\u0440\u043d\u044b\u0445 \u043a\u0438\u0441\u043b\u043e\u0442, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043e\u043f\u0442\u0438\u043c\u0438\u0437\u0438\u0440\u0443\u044e\u0442 \u0440\u0430\u0431\u043e\u0442\u0443 \u043a\u0440\u043e\u0432\u0435\u043d\u043e\u0441\u043d\u043e\u0439 \u0441\u0438\u0441\u0442\u0435\u043c\u044b, \u043e\u0447\u0438\u0449\u0430\u044f \u0435\u0435, \u0438 \u043f\u0440\u0435\u043f\u044f\u0442\u0441\u0442\u0432\u0443\u044e\u0442 \u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u043d\u0438\u044e \u0442\u0440\u043e\u043c\u0431\u043e\u0432. \u0420\u044b\u0431\u0438\u0439 \u0436\u0438\u0440 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u0431\u043e\u0433\u0430\u0442 \u0432\u0438\u0442\u0430\u043c\u0438\u043d\u0430\u043c\u0438 A, D \u0438 \u043f\u043e\u043b\u0435\u0437\u043d\u0435\u0435 \u0440\u0430\u0441\u0442\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u0436\u0438\u0440\u0430. \u041a\u0443\u043f\u0438\u0442\u044c \u0441\u0435\u043b\u044c\u0434\u044c \u0444\u0438\u043b\u0435 \u043e\u0442\u0431\u043e\u0440\u043d\u043e\u0435 \u041c\u0410\u0422\u0418\u0410\u0421 \u0421\u0430\u043d\u0442\u0430 \u0411\u0440\u0435\u043c\u043e\u0440 XXL, 300 \u0433 \u0432\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u041f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c: \u0417\u0410\u041e \u0420\u0443\u0441\u0441\u043a\u043e\u0435 \u043c\u043e\u0440\u0435, \u0420\u043e\u0441\u0441\u0438\u044f.\u0421\u043e\u0441\u0442\u0430\u0432: \u0444\u0438\u043b\u0435 \u0441\u0435\u043b\u044c\u0434\u0438, \u043c\u0430\u0441\u043b\u043e \u0440\u043e\u0441\u0442\u0438\u0442\u0435\u043b\u044c\u043d\u043e, \u0441\u043e\u043b\u044c, \u0434\u0435\u0441\u0442\u0440\u043e\u0437\u0430, \u0440\u0435\u0433\u0443\u043b\u044f\u0442\u043e\u0440 \u043a\u0438\u0441\u043b\u043e\u0442\u043d\u043e\u0441\u0442\u0438 \u043b\u0438\u043c\u043e\u043d\u043d\u0430\u044f \u043a\u0438\u0441\u043b\u043e\u0442\u0430, \u043a\u043e\u043d\u0441\u0435\u0440\u0432\u0430\u043d\u0442\u044b: \u0431\u0435\u043d\u0437\u0430\u0442 \u043d\u0430\u0442\u0440\u0438\u044f, \u0441\u043e\u0440\u0431\u0430\u0442 \u043a\u0430\u043b\u0438\u044f.\u041f\u0438\u0449\u0435\u0432\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: \u0431\u0435\u043b\u043a\u0438 \u2013 19 \u0433, \u0436\u0438\u0440\u044b \u2013 18 \u0433, \u0443\u0433\u043b\u0435\u0432\u043e\u0434\u044b \u2013 0 \u0433.\u042d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: 240 \u043a\u043a\u0430\u043b.\u0421\u0440\u043e\u043a \u0433\u043e\u0434\u043d\u043e\u0441\u0442\u0438: 4 \u043c\u0435\u0441\u044f\u0446\u0430.\u0423\u0441\u043b\u043e\u0432\u0438\u044f \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f: \u0445\u0440\u0430\u043d\u0438\u0442\u044c \u043f\u0440\u0438 \u0442\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0435 \u0434\u043e -8\u00b0\u0421.","9":"\u0428\u043e\u043a\u043e\u043b\u0430\u0434 METRO CHEF 60%, 1\u043a\u0433 \u043e\u0442\u043b\u0438\u0447\u0430\u0435\u0442\u0441\u044f \u0432\u043e\u0441\u0445\u0438\u0442\u0438\u0442\u0435\u043b\u044c\u043d\u044b\u043c \u0432\u043a\u0443\u0441\u043e\u043c \u0438 \u043d\u0435\u0436\u043d\u044b\u043c \u043f\u0440\u0438\u0442\u044f\u0433\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u043c \u0430\u0440\u043e\u043c\u0430\u0442\u043e\u043c \u0441 \u043d\u043e\u0442\u043a\u0430\u043c\u0438 \u043e\u0440\u0435\u0445\u043e\u0432. \u041f\u0440\u043e\u0434\u0443\u043a\u0442 \u0438\u0437\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d \u043f\u043e \u0442\u0440\u0430\u0434\u0438\u0446\u0438\u043e\u043d\u043d\u043e\u043c\u0443 \u0440\u0435\u0446\u0435\u043f\u0442\u0443 \u0441 \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u0435\u043c \u043d\u0430\u0442\u0443\u0440\u0430\u043b\u044c\u043d\u043e\u0439 \u043a\u0430\u043a\u0430\u043e-\u043f\u0430\u0441\u0442\u044b. \u0412 \u0441\u043e\u0441\u0442\u0430\u0432\u0435 \u043e\u0442\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u044e\u0442 \u0433\u0435\u043d\u043d\u043e\u043c\u043e\u0434\u0438\u0444\u0438\u0446\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0435 \u043a\u043e\u043c\u043f\u043e\u043d\u0435\u043d\u0442\u044b \u0438 \u0432\u0440\u0435\u0434\u043d\u044b\u0435 \u0434\u043e\u0431\u0430\u0432\u043a\u0438.\u0428\u043e\u043a\u043e\u043b\u0430\u0434 \u0438\u043c\u0435\u0435\u0442 \u043e\u0434\u043d\u043e\u0440\u043e\u0434\u043d\u0443\u044e \u0441\u0442\u0440\u0443\u043a\u0442\u0443\u0440\u0443. \u041e\u043d \u0442\u0430\u0435\u0442 \u0432\u043e \u0440\u0442\u0443, \u043e\u0441\u0442\u0430\u0432\u043b\u044f\u044f \u0431\u0430\u0440\u0445\u0430\u0442\u043d\u043e\u0435 \u043f\u043e\u0441\u043b\u0435\u0432\u043a\u0443\u0441\u0438\u0435. \u041d\u0435 \u043f\u0430\u0447\u043a\u0430\u0435\u0442 \u0440\u0443\u043a\u0438. \u0423\u0434\u043e\u0431\u043d\u0430\u044f \u0443\u043f\u0430\u043a\u043e\u0432\u043a\u0430 \u0437\u0430\u0449\u0438\u0449\u0430\u0435\u0442 \u043f\u0440\u043e\u0434\u0443\u043a\u0442 \u043e\u0442 \u0432\u043b\u0430\u0433\u0438. \u0428\u043e\u043a\u043e\u043b\u0430\u0434 METRO CHEF \u0441 \u0432\u044b\u0441\u043e\u043a\u0438\u043c \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u0435\u043c \u043a\u0430\u043a\u0430\u043e \u043c\u043e\u0436\u0435\u0442 \u0441\u0442\u0430\u0442\u044c \u043e\u0440\u0438\u0433\u0438\u043d\u0430\u043b\u044c\u043d\u044b\u043c \u0441\u043b\u0430\u0434\u043a\u0438\u043c \u043f\u043e\u0434\u0430\u0440\u043a\u043e\u043c. \u0423\u0434\u0438\u0432\u0438\u0442\u0435 \u0441\u0432\u043e\u0438\u0445 \u0431\u043b\u0438\u0437\u043a\u0438\u0445 \u0432\u043e\u043b\u0448\u0435\u0431\u043d\u044b\u043c \u0432\u043a\u0443\u0441\u043e\u043c \u0448\u043e\u043a\u043e\u043b\u0430\u0434\u0430 METRO CHEF 60%, 1\u043a\u0433, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0432\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u043f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO. \u0421\u043e\u0441\u0442\u0430\u0432: \u043a\u0430\u043a\u0430\u043e-\u043f\u0430\u0441\u0442\u0430, \u0441\u0430\u0445\u0430\u0440, \u043a\u0430\u043a\u0430\u043e-\u043c\u0430\u0441\u043b\u043e, \u044d\u043c\u0443\u043b\u044c\u0433\u0430\u0442\u043e\u0440 \u0441\u043e\u0435\u0432\u044b\u0439 \u043b\u0435\u0446\u0438\u0442\u0438\u043d, \u0430\u0440\u043e\u043c\u0430\u0442\u0438\u0437\u0430\u0442\u043e\u0440 \u0438\u0434\u0435\u043d\u0442\u0438\u0447\u043d\u044b\u0439 \u043d\u0430\u0442\u0443\u0440\u0430\u043b\u044c\u043d\u043e\u043c\u0443 \u0432\u0430\u043d\u0438\u043b\u0438\u043d.\u041f\u0438\u0449\u0435\u0432\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c \u0432 100 \u0433 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u0430: \u0431\u0435\u043b\u043a\u0438 7 \u0433, \u0443\u0433\u043b\u0435\u0432\u043e\u0434\u044b 43,4 \u0433, \u0436\u0438\u0440\u044b\u00a0 40 \u0433.\u042d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c \u2013 577\u043a\u043a\u0430\u043b.\u041f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c: \u0418\u0442\u0430\u043b\u0438\u044f.\u0412\u0435\u0441: 1 \u043a\u0433."},"images":{"0":["https:\/\/cdn.metro-cc.ru\/ru\/ru_pim_261202001001_01.png"],"1":