43d\u043e \u0438\u043b\u0438 \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0433\u0430\u0440\u043d\u0438\u0440\u0430 \u043a \u043c\u044f\u0441\u0443, \u043f\u0442\u0438\u0446\u0435 \u0438\u043b\u0438 \u0440\u044b\u0431\u0435, \u0441\u043e\u0447\u0435\u0442\u0430\u0442\u044c \u0441\u043e \u0441\u0432\u0435\u0436\u0438\u043c\u0438, \u043c\u0430\u0440\u0438\u043d\u043e\u0432\u0430\u043d\u043d\u044b\u043c\u0438 \u043e\u0432\u043e\u0449\u0430\u043c\u0438, \u0433\u0440\u0438\u0431\u0430\u043c\u0438, \u0441\u043e\u043b\u0435\u043d\u044c\u044f\u043c\u0438 \u0438 \u0437\u0435\u043b\u0435\u043d\u044c\u044e. \u0423\u0434\u0430\u0447\u043d\u044b\u043c \u0441\u043e\u043f\u0440\u043e\u0432\u043e\u0436\u0434\u0435\u043d\u0438\u0435\u043c \u043a \u044d\u0442\u043e\u043c\u0443 \u0431\u043b\u044e\u0434\u0443 \u0441\u0442\u0430\u043d\u0435\u0442 \u0441\u044b\u0440\u043d\u044b\u0439, \u0447\u0435\u0441\u043d\u043e\u0447\u043d\u044b\u0439, \u0433\u0440\u0438\u0431\u043d\u043e\u0439 \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u0441\u043e\u0443\u0441\u044b. \u041a\u0430\u0440\u0442\u043e\u0444\u0435\u043b\u044c\u043d\u044b\u0435 \u0434\u043e\u043b\u044c\u043a\u0438 \u0431\u044b\u0441\u0442\u0440\u043e \u0443\u0442\u043e\u043b\u044f\u044e\u0442 \u0433\u043e\u043b\u043e\u0434, \u0434\u0430\u0440\u044f\u0442 \u0437\u0430\u0440\u044f\u0434 \u0441\u0438\u043b \u0438 \u044d\u043d\u0435\u0440\u0433\u0438\u0438 \u0438 \u0445\u043e\u0440\u043e\u0448\u043e \u0443\u0441\u0432\u0430\u0438\u0432\u0430\u044e\u0442\u0441\u044f, \u043d\u0435 \u0432\u044b\u0437\u044b\u0432\u0430\u044f \u043d\u0435\u043f\u0440\u0438\u044f\u0442\u043d\u043e\u0433\u043e \u0447\u0443\u0432\u0441\u0442\u0432\u0430 \u0442\u044f\u0436\u0435\u0441\u0442\u0438 \u0432 \u0436\u0435\u043b\u0443\u0434\u043a\u0438. \u0423\u043f\u043e\u0442\u0440\u0435\u0431\u043b\u0435\u043d\u0438\u0435 \u044d\u0442\u043e\u0433\u043e \u043e\u0432\u043e\u0449\u0430 \u043f\u043e\u043b\u0435\u0437\u043d\u043e \u0434\u043b\u044f \u0437\u0434\u043e\u0440\u043e\u0432\u044c\u044f, \u043f\u043e\u0441\u043a\u043e\u043b\u044c\u043a\u0443 \u0432 \u0435\u0433\u043e \u0441\u043e\u0441\u0442\u0430\u0432 \u0432\u0445\u043e\u0434\u044f\u0442 \u0432\u0438\u0442\u0430\u043c\u0438\u043d\u044b \u0438\u0437 \u0433\u0440\u0443\u043f\u043f\u044b \u0412, \u0420\u0420 \u0438 \u0421, \u043a\u0430\u043b\u0438\u0439, \u043c\u0430\u0433\u043d\u0438\u0439, \u0446\u0438\u043d\u043a \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u043c\u0438\u043a\u0440\u043e- \u0438 \u043c\u0430\u043a\u0440\u043e\u044d\u043b\u0435\u043c\u0435\u043d\u0442\u044b. \u0412\u043a\u043b\u044e\u0447\u0435\u043d\u0438\u0435 \u043a\u0430\u0440\u0442\u043e\u0444\u0435\u043b\u044f \u0432 \u0440\u0435\u0433\u0443\u043b\u044f\u0440\u043d\u043e\u0435 \u043c\u0435\u043d\u044e \u0443\u043b\u0443\u0447\u0448\u0430\u0435\u0442 \u043f\u0438\u0449\u0435\u0432\u0430\u0440\u0435\u043d\u0438\u0435, \u0430\u043a\u0442\u0438\u0432\u0438\u0437\u0438\u0440\u0443\u0435\u0442 \u0440\u0430\u0431\u043e\u0442\u0443 \u043c\u043e\u0437\u0433\u0430, \u0441\u043f\u043e\u0441\u043e\u0431\u0441\u0442\u0432\u0443\u0435\u0442 \u0432\u044b\u0432\u043e\u0434\u0443 \u0442\u043e\u043a\u0441\u0438\u043d\u043e\u0432 \u0438 \u043b\u0438\u0448\u043d\u0435\u0439 \u0436\u0438\u0434\u043a\u043e\u0441\u0442\u0438 \u0438\u0437 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043c\u0430, \u0441\u043d\u0438\u0436\u0430\u0435\u0442 \u0440\u0438\u0441\u043a \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u044f \u0431\u043e\u043b\u0435\u0437\u043d\u0435\u0439 \u0441\u0435\u0440\u0434\u0446\u0430. \u041f\u0440\u043e\u0434\u0443\u043a\u0442 \u043f\u043e\u043c\u0435\u0449\u0435\u043d \u0432 \u0437\u0430\u0449\u0438\u0442\u043d\u0443\u044e \u0443\u043f\u0430\u043a\u043e\u0432\u043a\u0443 \u0444\u043b\u043e\u0443-\u043f\u0430\u043a \u0438 \u0440\u0430\u0441\u0444\u0430\u0441\u043e\u0432\u0430\u043d \u043d\u0430 \u043f\u0430\u0440\u0442\u0438\u0438 \u0432\u0435\u0441\u043e\u043c 2,5 \u043a\u0433 \u2013 \u0442\u0430\u043a\u043e\u0433\u043e \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u0430 \u0445\u0432\u0430\u0442\u0438\u0442, \u0447\u0442\u043e\u0431\u044b \u043f\u0440\u0438\u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c \u0433\u0430\u0440\u043d\u0438\u0440 \u0434\u0430\u0436\u0435 \u043d\u0430 \u0431\u043e\u043b\u044c\u0448\u043e\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u0435\u0440\u0441\u043e\u043d.\u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u043a\u0430\u0440\u0442\u043e\u0444\u0435\u043b\u044c\u043d\u044b\u0435 \u0434\u043e\u043b\u044c\u043a\u0438 \u0441\u043e \u0441\u043f\u0435\u0446\u0438\u044f\u043c\u0438 METRO CHEF, 2,5 \u043a\u0433 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u0412\u0438\u0434 \u043e\u0432\u043e\u0449\u0435\u0439: \u043a\u0430\u0440\u0442\u043e\u0444\u0435\u043b\u044c.\u0422\u0438\u043f \u043e\u0431\u0440\u0430\u0431\u043e\u0442\u043a\u0438: \u0440\u0435\u0437\u0430\u043d\u044b\u0439.\u041e\u0441\u043e\u0431\u0435\u043d\u043d\u043e\u0441\u0442\u0438: \u0441 \u043a\u043e\u0436\u0443\u0440\u043e\u0439.\u0421\u043e\u0441\u0442\u0430\u0432: \u043f\u0430\u043b\u044c\u043c\u043e\u0432\u043e\u0435 \u043c\u0430\u0441\u043b\u043e, \u0434\u0435\u043a\u0441\u0442\u0440\u043e\u0437\u0430, \u043a\u0430\u0440\u0442\u043e\u0444\u0435\u043b\u044c 94%, \u043c\u043e\u0434\u0438\u0444\u0438\u0446\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u044b\u0439 \u043a\u0430\u0440\u0442\u043e\u0444\u0435\u043b\u044c\u043d\u044b\u0439 \u043a\u0440\u0430\u0445\u043c\u0430\u043b, \u0441\u043e\u043b\u044c, \u043f\u0435\u0440\u0435\u0446, \u043f\u0435\u0440\u0435\u0446 (\u0441\u043f\u0435\u0446\u0438\u0438), \u043f\u0435\u0440\u0435\u0446, \u043b\u0443\u043a\u043e\u0432\u044b\u0439 \u043f\u043e\u0440\u043e\u0448\u043e\u043a, \u0447\u0435\u0441\u043d\u043e\u0447\u043d\u044b\u0439 \u043f\u043e\u0440\u043e\u0448\u043e\u043a, \u044d\u043a\u0441\u0442\u0440\u0430\u043a\u0442 \u0434\u0440\u043e\u0436\u0436\u0435\u0439, \u043a\u0430\u0440\u0431\u043e\u043d\u0430\u0442\u044b \u043d\u0430\u0442\u0440\u0438\u044f.\u0412\u0435\u0441: 2,5 \u043a\u0433.\u0422\u0438\u043f \u0443\u043f\u0430\u043a\u043e\u0432\u043a\u0438: \u043f\u0430\u043a\u0435\u0442.\u041f\u0438\u0449\u0435\u0432\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: \u0431\u0435\u043b\u043a\u0438 2,2 \u0433, \u0436\u0438\u0440\u044b 3,8 \u0433, \u0443\u0433\u043b\u0435\u0432\u043e\u0434\u044b 19 \u0433.\u042d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: 124 \u043a\u043a\u0430\u043b.\u0423\u0441\u043b\u043e\u0432\u0438\u044f \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f: \u0445\u0440\u0430\u043d\u0438\u0442\u044c \u043f\u0440\u0438 \u0442\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0435 \u043e\u043a\u043e\u043b\u043e -18\u00b0\u0421.\u0421\u0440\u043e\u043a \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f: 18 \u043c\u0435\u0441\u044f\u0446\u0435\u0432.\u0411\u0440\u0435\u043d\u0434: Metro Chef.\u041f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c: Metro C&C.\u0421\u0442\u0440\u0430\u043d\u0430-\u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c: \u0420\u043e\u0441\u0441\u0438\u044f.","5":"","6":"\u041f\u0440\u043e\u0444\u0438\u0442\u0440\u043e\u043b\u0438 METRO CHEF, 1,2 \u043a\u0433 \u2014 \u044d\u0442\u043e \u043d\u0435\u0432\u0435\u0440\u043e\u044f\u0442\u043d\u043e \u0432\u043a\u0443\u0441\u043d\u044b\u0439 \u0434\u0435\u0441\u0435\u0440\u0442, \u0438\u0437\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u043d\u044b\u0439 \u043d\u0430 \u043e\u0441\u043d\u043e\u0432\u0435 \u0437\u0430\u0432\u0430\u0440\u043d\u043e\u0433\u043e \u0442\u0435\u0441\u0442\u0430. \u041f\u0440\u043e\u0434\u0443\u043a\u0442 \u0437\u0430\u043c\u043e\u0440\u043e\u0436\u0435\u043d\u043d\u044b\u0439, \u0443\u043f\u0430\u043a\u043e\u0432\u0430\u043d \u0432 \u043f\u043b\u0430\u0441\u0442\u0438\u043a\u043e\u0432\u044b\u0439 \u043a\u043e\u043d\u0442\u0435\u0439\u043d\u0435\u0440 \u0438 \u043a\u0430\u0440\u0442\u043e\u043d\u043d\u0443\u044e \u043a\u043e\u0440\u043e\u0431\u043a\u0443. \u0412\u043d\u0443\u0442\u0440\u0438 \u043a\u0430\u0436\u0434\u043e\u0433\u043e \u0448\u0430\u0440\u0438\u043a\u0430 \u043d\u0430\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u043d\u0435\u0436\u043d\u0435\u0439\u0448\u0438\u0439 \u043a\u0440\u0435\u043c, \u0441\u0432\u0435\u0440\u0445\u0443 \u043e\u043d\u0438 \u043f\u043e\u043b\u0438\u0442\u044b \u0432\u043a\u0443\u0441\u043d\u044b\u043c \u0441\u043e\u0443\u0441\u043e\u043c. \u041f\u043e\u0441\u043b\u0435 \u0440\u0430\u0437\u043c\u043e\u0440\u0430\u0436\u0438\u0432\u0430\u043d\u0438\u044f \u043f\u0440\u043e\u0444\u0438\u0442\u0440\u043e\u043b\u0438 \u0433\u043e\u0442\u043e\u0432\u044b \u043a \u0443\u043f\u043e\u0442\u0440\u0435\u0431\u043b\u0435\u043d\u0438\u044e. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u043f\u0440\u043e\u0444\u0438\u0442\u0440\u043e\u043b\u0438 METRO CHEF, 1,2 \u043a\u0433 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u0421\u043e\u0441\u0442\u0430\u0432: \u0437\u0430\u0432\u0430\u0440\u043d\u043e\u0435 \u0442\u0435\u0441\u0442\u043e, \u043a\u0440\u0435\u043c.\u042d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: 449 \u043a\u043a\u0430\u043b.\u041f\u0438\u0449\u0435\u0432\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: \u0431\u0435\u043b\u043a\u0438 \u2014 7,1 \u0433, \u0436\u0438\u0440\u044b \u2014 32,9 \u0433, \u0443\u0433\u043b\u0435\u0432\u043e\u0434\u044b \u2014 31,6.\u0421\u0440\u043e\u043a \u0433\u043e\u0434\u043d\u043e\u0441\u0442\u0438: \u043f\u0440\u0438 \u0442\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0435 -18\u00b0\u0421 2 \u0433\u043e\u0434\u0430.\u0421\u0442\u0440\u0430\u043d\u0430-\u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c: \u0420\u043e\u0441\u0441\u0438\u044f.","7":" \u041a\u0430\u043f\u0443\u0441\u0442\u0430 \u0431\u0440\u043e\u043a\u043a\u043e\u043b\u0438 FINE LIFE 20\/40, 400 \u0433 \u2013 \u044d\u0442\u043e \u043f\u043e\u043b\u0443\u0444\u0430\u0431\u0440\u0438\u043a\u0430\u0442 \u0432 \u0432\u0430\u043a\u0443\u0443\u043c\u043d\u043e\u0439 \u0443\u043f\u0430\u043a\u043e\u0432\u043a\u0435, \u043f\u0440\u0435\u0434\u043d\u0430\u0437\u043d\u0430\u0447\u0435\u043d\u043d\u044b\u0439 \u0434\u043b\u044f \u043f\u0440\u0438\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u0438\u044f \u0440\u0430\u0437\u043d\u044b\u0445 \u0431\u043b\u044e\u0434. \u0418\u0437 \u043d\u0435\u0433\u043e \u043c\u043e\u0436\u043d\u043e \u0431\u044b\u0441\u0442\u0440\u043e \u043f\u0440\u0438\u0433\u043e\u0442\u043e\u0432\u0438\u0442\u044c \u0432\u043a\u0443\u0441\u043d\u044b\u0439 \u043e\u0431\u0435\u0434 \u0438\u043b\u0438 \u0443\u0436\u0438\u043d. \u0414\u043e\u0441\u0442\u0430\u0442\u043e\u0447\u043d\u043e \u0440\u0430\u0437\u043c\u043e\u0440\u043e\u0437\u0438\u0442\u044c \u0443\u043f\u0430\u043a\u043e\u0432\u043a\u0443 \u0438 \u0441\u0440\u0430\u0437\u0443 \u0436\u0435 \u043f\u0440\u0438\u0441\u0442\u0443\u043f\u0430\u0442\u044c \u043a \u043f\u0440\u0438\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u0438\u044e \u0432\u044b\u0431\u0440\u0430\u043d\u043d\u043e\u0433\u043e \u0431\u043b\u044e\u0434\u0430. \u0411\u0440\u043e\u043a\u043a\u043e\u043b\u0438 \u0435\u0434\u044f\u0442 \u0441\u044b\u0440\u043e\u0439, \u0437\u0430\u043f\u0435\u043a\u0430\u044e\u0442, \u0432\u0430\u0440\u044f\u0442, \u0433\u043e\u0442\u043e\u0432\u044f\u0442 \u043d\u0430 \u043f\u0430\u0440\u0443, \u0436\u0430\u