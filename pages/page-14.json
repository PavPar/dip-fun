0430 \u0442\u0430\u043a\u0436\u0435 \u043a\u043e\u0431\u0430\u043b\u044c\u0442 \u0438 \u043c\u0430\u0440\u0433\u0430\u043d\u0435\u0446, \u043c\u0435\u0434\u044c \u0438 \u043c\u043e\u043b\u0438\u0431\u0434\u0435\u043d, \u0444\u0442\u043e\u0440 \u0438 \u0446\u0438\u043d\u043a. \u041e\u043d\u0438 \u0431\u043e\u0433\u0430\u0442\u044b \u0432\u0438\u0442\u0430\u043c\u0438\u043d\u0430\u043c\u0438 \u041a, \u0410, \u0415, D, \u0421, \u04121, \u04122, \u04129, \u0420\u0420, \u043f\u0440\u043e\u0432\u0438\u0442\u0430\u043c\u0438\u043d \u0410 \u0438 B-\u043a\u0430\u0440\u043e\u0442\u0438\u043d. \u0412 \u043d\u0438\u0445 \u043c\u043d\u043e\u0433\u043e \u0431\u0435\u043b\u043a\u0430, \u043f\u0440\u0438 \u044d\u0442\u043e\u043c \u043f\u0440\u043e\u0434\u0443\u043a\u0442 \u044f\u0432\u043b\u044f\u0435\u0442\u0441\u044f \u043d\u0438\u0437\u043a\u043e\u043a\u0430\u043b\u043e\u0440\u0438\u0439\u043d\u044b\u043c. \u041a\u0440\u0435\u0432\u0435\u0442\u043a\u0438 \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u043f\u043e\u043b\u0435\u0437\u043d\u044b, \u043d\u043e \u0438 \u043d\u0435\u043e\u0431\u044b\u0447\u0430\u0439\u043d\u043e \u0432\u043a\u0443\u0441\u043d\u044b. \u0418\u0445 \u043c\u043e\u0436\u043d\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u043a\u0430\u043a \u0441\u0430\u043c\u043e\u0441\u0442\u043e\u044f\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0434\u0435\u043b\u0438\u043a\u0430\u0442\u0435\u0441 \u0438\u043b\u0438 \u043f\u0440\u0438\u043c\u0435\u043d\u044f\u0442\u044c \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0438\u043d\u0433\u0440\u0435\u0434\u0438\u0435\u043d\u0442\u0430 \u0434\u043b\u044f \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u0445 \u0431\u043b\u044e\u0434 (\u0434\u043b\u044f \u0441\u0430\u043b\u0430\u0442\u043e\u0432, \u0441\u0443\u0448\u0438, \u043f\u0438\u0446\u0446\u044b, \u043c\u043e\u0440\u0441\u043a\u0438\u0445 \u043a\u043e\u043a\u0442\u0435\u0439\u043b\u0435\u0439 \u0438 \u0442.\u0434.). \u0418\u0445 \u043e\u0442\u0432\u0430\u0440\u0438\u0432\u0430\u044e\u0442, \u0436\u0430\u0440\u044f\u0442 \u0438\u043b\u0438 \u0437\u0430\u043f\u0435\u043a\u0430\u044e\u0442. \u041c\u044f\u0441\u043e \u043a\u0440\u0435\u0432\u0435\u0442\u043e\u043a \u0441\u043e\u0445\u0440\u0430\u043d\u044f\u0435\u0442 \u0441\u0432\u043e\u0439 \u043f\u0435\u0440\u0432\u043e\u043d\u0430\u0447\u0430\u043b\u044c\u043d\u044b\u0439 \u0432\u043a\u0443\u0441 \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u0442\u0435\u0445\u043d\u043e\u043b\u043e\u0433\u0438\u0438 \u0437\u0430\u043c\u043e\u0440\u043e\u0437\u043a\u0438. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u043a\u0440\u0435\u0432\u0435\u0442\u043a\u0438 \u0442\u0438\u0433\u0440\u043e\u0432\u044b\u0435 Metro Chef 30\/40 \u0441 \u0433\u043e\u043b\u043e\u0432\u043e\u0439, 800\u0433 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO. METRO Chef \u2013 \u044d\u0442\u043e \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u044b\u0439 \u0431\u0440\u0435\u043d\u0434, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u043f\u0440\u0438\u043d\u0430\u0434\u043b\u0435\u0436\u0438\u0442 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u0439 \u0441\u0435\u0442\u0438 \u041c\u0435\u0442\u0440\u043e. \u0412 \u0430\u0441\u0441\u043e\u0440\u0442\u0438\u043c\u0435\u043d\u0442 \u0432\u0445\u043e\u0434\u044f\u0442 \u0442\u0430\u043a\u0438\u0435 \u043a\u0430\u0442\u0435\u0433\u043e\u0440\u0438\u0438 \u0442\u043e\u0432\u0430\u0440\u043e\u0432, \u043a\u0430\u043a \u043c\u043e\u043b\u043e\u0447\u043d\u044b\u0435 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u044b, \u043c\u044f\u0441\u043d\u0430\u044f \u0433\u0430\u0441\u0442\u0440\u043e\u043d\u043e\u043c\u0438\u044f, \u0441\u043e\u0443\u0441\u044b \u0438 \u043f\u0440\u0438\u043f\u0440\u0430\u0432\u044b, \u0437\u0430\u043c\u043e\u0440\u043e\u0436\u0435\u043d\u043d\u044b\u0435 \u0444\u0440\u0443\u043a\u0442\u044b \u0438 \u044f\u0433\u043e\u0434\u044b, \u0440\u044b\u0431\u0430 \u0438 \u043c\u043e\u0440\u0435\u043f\u0440\u043e\u0434\u0443\u043a\u0442\u044b \u0438 \u043c\u043d\u043e\u0433\u043e\u0435 \u0434\u0440\u0443\u0433\u043e\u0435. \u0412\u044b\u043f\u0443\u0441\u043a\u0430\u044f \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044e \u043f\u043e\u0434 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c\u0438 \u0442\u043e\u0440\u0433\u043e\u0432\u044b\u043c\u0438 \u043c\u0430\u0440\u043a\u0430\u043c\u0438 METRO, \u043c\u044b \u0441\u0442\u0440\u0435\u043c\u0438\u043c\u0441\u044f \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u0443\u0434\u043e\u0432\u043b\u0435\u0442\u0432\u043e\u0440\u0438\u0442\u044c \u043f\u043e\u0442\u0440\u0435\u0431\u043d\u043e\u0441\u0442\u0438 \u043d\u0430\u0448\u0438\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432. \u041d\u0430\u0448\u0430 \u0446\u0435\u043b\u044c \u2013 \u043f\u043e\u043d\u044f\u0442\u044c, \u043a\u0430\u043a\u043e\u0439 \u0430\u0441\u0441\u043e\u0440\u0442\u0438\u043c\u0435\u043d\u0442 \u0434\u043b\u044f \u043d\u0438\u0445 \u0430\u043a\u0442\u0443\u0430\u043b\u0435\u043d \u0438 \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0438\u0442\u044c \u0435\u0433\u043e \u043f\u043e \u043f\u0440\u0438\u0432\u043b\u0435\u043a\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u0446\u0435\u043d\u0435. \u041c\u044b \u0442\u0430\u043a\u0436\u0435 \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0438\u0437\u0443\u0447\u0430\u0435\u043c \u0442\u0435\u043d\u0434\u0435\u043d\u0446\u0438\u0438 \u0440\u044b\u043d\u043a\u0430, \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u0442\u044c \u0432 \u043a\u0443\u0440\u0441\u0435 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u0442\u0440\u0435\u043d\u0434\u043e\u0432 \u0438 \u0441\u0432\u043e\u0435\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0442\u044c \u0440\u0435\u0448\u0435\u043d\u0438\u044f \u043f\u043e\u0434 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c\u0438 \u0442\u043e\u0440\u0433\u043e\u0432\u044b\u043c\u0438 \u043c\u0430\u0440\u043a\u0430\u043c\u0438 \u0432 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0438 \u0441 \u043d\u0438\u043c\u0438.","3":"\u041e\u0431\u043b\u0435\u043f\u0438\u0445\u0430 \u0431\u044b\u0441\u0442\u0440\u043e\u0437\u0430\u043c\u043e\u0440\u043e\u0436\u0435\u043d\u043d\u0430\u044f METRO CHEF, 1,5 \u043a\u0433 \u0431\u0443\u0434\u0435\u0442 \u043e\u043f\u0442\u0438\u043c\u0430\u043b\u044c\u043d\u044b\u043c \u0440\u0435\u0448\u0435\u043d\u0438\u0435\u043c \u0434\u043b\u044f \u0434\u043e\u043c\u0430, \u0440\u0435\u0441\u0442\u043e\u0440\u0430\u043d\u043e\u0432, \u0433\u043e\u0441\u0442\u0438\u043d\u0438\u0446 \u0437\u0430 \u0441\u0447\u0435\u0442 \u0434\u043b\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u0441\u0440\u043e\u043a\u0430 \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f. \u0421\u043f\u0435\u043b\u044b\u0439 \u0446\u0435\u043b\u044b\u0435 \u044f\u0433\u043e\u0434\u044b \u0431\u043b\u0430\u0433\u043e\u0434\u0430\u0440\u044f \u043c\u043e\u043c\u0435\u043d\u0442\u0430\u043b\u044c\u043d\u043e \u0437\u0430\u043c\u043e\u0440\u043e\u0437\u043a\u0435 \u0441\u043e\u0445\u0440\u0430\u043d\u044f\u044e\u0442 \u0432\u0441\u0435 \u0441\u0432\u043e\u0438 \u043f\u043e\u043b\u0435\u0437\u043d\u044b\u0435 \u0441\u0432\u043e\u0439\u0441\u0442\u0432\u0430, \u0430 \u0441\u043e\u0434\u0435\u0440\u0436\u0430\u043d\u0438\u0435 \u0432\u0438\u0442\u0430\u043c\u0438\u043d\u0430 \u0421 \u0432 \u0437\u0430\u043c\u043e\u0440\u043e\u0436\u0435\u043d\u043d\u043e\u0439 \u043e\u0431\u043b\u0435\u043f\u0438\u0445\u0435 \u0433\u043e\u0440\u0430\u0437\u0434\u043e \u0432\u044b\u0448\u0435, \u0447\u0435\u043c \u0432 \u0441\u0432\u0435\u0436\u0435\u0439. \u042f\u0433\u043e\u0434\u0430 \u0438\u0437\u0432\u0435\u0441\u0442\u043d\u0430 \u0441\u0432\u043e\u0438\u043c \u043e\u0431\u0449\u0435\u0443\u043a\u0440\u0435\u043f\u043b\u044f\u044e\u0449\u0438\u043c \u0438 \u043e\u043c\u043e\u043b\u0430\u0436\u0438\u0432\u0430\u044e\u0449\u0438\u043c \u0434\u0435\u0439\u0441\u0442\u0432\u0438\u0435\u043c. \u041e\u043d\u0430 \u043f\u043e\u043b\u043e\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442\u0441\u044f \u043d\u0430 \u0438\u043c\u043c\u0443\u043d\u0438\u0442\u0435\u0442\u0435, \u0437\u0440\u0435\u043d\u0438\u0438, \u0437\u0434\u043e\u0440\u043e\u0432\u044c\u0435 \u043a\u043e\u0436\u0435 \u0438 \u0434\u0430\u0436\u0435 \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u0442 \u0441\u0435\u0440\u043e\u0442\u043e\u043d\u0438\u043d \u2013 \u0433\u043e\u0440\u043c\u043e\u043d \u0441\u0447\u0430\u0441\u0442\u044c\u044f. \u0418\u0437 \u044f\u0433\u043e\u0434 \u0432\u044b\u0439\u0434\u0435\u0442 \u0432\u0430\u0440\u0435\u043d\u044c\u0435 \u0438\u043b\u0438 \u0434\u0436\u0435\u043c, \u043e\u043d\u0430 \u0445\u043e\u0440\u043e\u0448\u0430 \u0438 \u043f\u0440\u043e\u0441\u0442\u043e \u0432 \u0440\u0430\u0437\u043c\u043e\u0440\u043e\u0436\u0435\u043d\u043d\u043e\u043c \u0432\u0438\u0434\u0435. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u043e\u0431\u043b\u0435\u043f\u0438\u0445\u0443 \u0431\u044b\u0441\u0442\u0440\u043e\u0437\u0430\u043c\u043e\u0440\u043e\u0436\u0435\u043d\u043d\u0443\u044e METRO CHEF, 1,5 \u043a\u0433 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u0412\u0435\u0441: 1,5 \u043a\u0433.\u0423\u0441\u043b\u043e\u0432\u0438\u044f \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f: \u0434\u043e -18 \u00b0\u0421.\u0421\u0440\u043e\u043a \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f: 1 \u0433\u043e\u0434.<iframe allow=accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture allowfullscreen= frameborder=0 height=315 src=https:\/\/www.youtube.com\/embed\/GfNJhRESKGk?list=PLJA09ShK5QA1MJnxboE9tgtfIPUiZjPYT width=560><\/iframe><iframe allow=accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture allowfullscreen= frameborder=0 height=315 src=https:\/\/www.youtube.com\/embed\/nMfxtV7MzRk?list=PLJA09ShK5QA1MJnxboE9tgtfIPUiZjPYT width=560><\/iframe>","4":"\u0414\u043e\u043d\u0430\u0442 RIOBA \u0442\u0435\u043c\u043d\u0430\u044f \u0433\u043b\u0430\u0437\u0443\u0440\u044c, 12\u044552\u0433 \u2014 \u044d\u0442\u043e \u043f\u043e\u043d\u0447\u0438\u043a \u0438\u0437 \u0434\u0440\u043e\u0436\u0436\u0435\u0432\u043e\u0433\u043e \u0442\u0435\u0441\u0442\u0430 \u043f\u043e\u043a\u0440\u044b\u0442\u044b\u0439 \u0441\u0432\u0435\u0440\u0445\u0443 \u0433\u043b\u0430\u0437\u0443\u0440\u044c\u044e \u0438\u0445 \u0442\u0435\u043c\u043d\u043e\u0433\u043e \u0448\u043e\u043a\u043e\u043b\u0430\u0434\u0430. \u041f\u0440\u0438 \u043f\u043e\u043a\u0443\u043f\u043a\u0435 \u044d\u0442\u043e\u0433\u043e \u0434\u0435\u0441\u0435\u0440\u0442\u0430 \u0432 \u0443\u043f\u0430\u043a\u043e\u0432\u043a\u0435 \u0435\u0433\u043e \u0445\u0432\u0430\u0442\u0438\u0442 \u043d\u0430 \u0446\u0435\u043b\u0443\u044e \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u044e. \u041e\u043d \u0438\u043c\u0435\u0435\u0442 \u043f\u0440\u0438\u044f\u0442\u043d\u044b\u0439 \u0441\u043b\u0430\u0434\u043a\u0438\u0439 \u0432\u043a\u0443\u0441 \u0438 \u043f\u043e\u0434\u0445\u043e\u0434\u0438\u0442 \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0434\u0435\u0441\u0435\u0440\u0442\u0430 \u043a \u043b\u044e\u0431\u043e\u043c\u0443 \u043d\u0430\u043f\u0438\u0442\u043a\u0443. \u041f\u0435\u0440\u0435\u0434 \u0443\u043f\u043e\u0442\u0440\u0435\u0431\u043b\u0435\u043d\u0438\u0435\u043c \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u0435\u0442\u0441\u044f \u0440\u0430\u0437\u043c\u043e\u0440\u043e\u0437\u0438\u0442\u044c \u0435\u0433\u043e \u043f\u0440\u0438 \u043a\u043e\u043c\u043d\u0430\u0442\u043d\u043e\u0439 \u0442\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0435 \u0432 \u0442\u0435\u0447\u0435\u043d\u0438\u0435 \u0447\u0430\u0441\u0430. \u041f\u043e\u0441\u043b\u0435 \u0440\u0430\u0437\u043c\u043e\u0440\u0430\u0436\u0438\u0432\u0430\u043d\u0438\u044f \u043d\u0435 \u0440\u0435\u043a\u043e\u043c\u0435\u043d\u0434\u0443\u0435\u0442\u0441\u044f \u0435\u0433\u043e \u0437\u0430\u043c\u043e\u0440\u0430\u0436\u0438\u0432\u0430\u0442\u044c \u0441\u043d\u043e\u0432\u0430. \u0412 \u0435\u0433\u043e \u0441\u043e\u0441\u0442\u0430\u0432 \u043d\u0435 \u0432\u0445\u043e\u0434\u044f\u0442 \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u0435 \u043a\u043e\u043d\u0441\u0435\u0440\u0432\u0430\u043d\u0442\u044b \u0438 \u043f\u043e\u0434\u0441\u043b\u0430\u0441\u0442\u0438\u0442\u0435\u043b\u0438. \u041d\u0435 \u0441\u043e\u0434\u0435\u0440\u0436\u0438\u0442 \u0433\u0438\u0434\u0440\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u043e\u0433\u043e \u0440\u0430\u0441\u0442\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u043c\u0430\u0441\u043b\u0430. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u0414\u043e\u043d\u0430\u0442 RIOBA \u0442\u0435\u043c\u043d\u0430\u044f \u0433\u043b\u0430\u0437\u0443\u0440\u044c, 12\u044552\u0433 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u0422\u0438\u043f: \u043a\u043e\u043d\u0434\u0438\u0442\u0435\u0440\u0441\u043a\u043e\u0435 \u0438\u0437\u0434\u0435\u043b\u0438\u0435.\u0421\u043e\u0441\u0442\u0430\u0432: \u043f\u0448\u0435\u043d\u0438\u0447\u043d\u0430\u044f \u043c\u0443\u043a\u0430, \u043c\u0430\u0441\u043b\u043e \u0440\u0430\u0441\u0442\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0435, \u0432\u043e\u0434\u0430, \u0441\u0430\u0445\u0430\u0440, \u0434\u0440\u043e\u0436\u0436\u0438, \u043a\u0430\u043a\u0430\u043e-\u043c\u0430\u0441\u0441\u0430, \u044d\u043c\u0443\u043b\u044c\u0433\u0430\u0442\u043e\u0440\u044b: \u043c\u043e\u043d\u043e- \u0438 \u0434\u0438\u0433\u043b\u0438\u0446\u0438\u0440\u0438\u0434\u044b \u0436\u0438\u0440\u043d\u044b\u0445 \u043a\u0438\u0441\u043b\u043e\u0442 \u0415471, \u043b\u0435\u0446\u0438\u0442\u0438\u043d (\u0441\u043e\u044f) \u0415322, \u043e\u0431\u0435\u0437\u0436\u0438\u0440\u0435\u043d\u043d\u044b\u0439 \u043a\u0430\u043a\u0430\u043e-\u043f\u043e\u0440\u043e\u0448\u043e\u043a, \u043c\u043e\u043b\u043e\u0447\u043d\u044b\u0439 \u0441\u0430\u0445\u0430\u0440, \u0441\u0443\u0445\u043e\u0439 \u044f\u0438\u0447\u043d\u044b\u0439 \u0436\u0435\u043b\u0442\u043e\u043a, \u0440\u0430\u0441\u0442\u0438\u0442\u0435\u043b\u044c\u043d\u043e\u0435 \u043c\u0430\u0441\u043b\u043e, \u0441\u043e\u043b\u044c \u043f\u0438\u0449\u0435\u0432\u0430\u044f, \u0441\u043e\u0435\u0432\u0430\u044f \u043c\u0443\u043a\u0430, \u0441\u0443\u0445\u043e\u0435 \u043e\u0431\u0435\u0437\u0436\u0438\u0440\u0435\u043d\u043d\u043e\u0435 \u043c\u043e\u043b\u043e\u043a\u043e, \u0430\u0440\u043e\u043c\u0430\u0442\u0438\u0437\u0430\u0442\u043e\u0440 \u0438\u0434\u0435\u043d\u0442\u0438\u0447\u043d\u044b\u0439 \u043d\u0430\u0442\u0443\u0440\u0430\u043b\u044c\u043d\u043e\u043c\u0443.\u041f\u0438\u0449\u0435\u0432\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c \u043d\u0430 100 \u0433: \u0431\u0435\u043b\u043a\u0438 5,7 \u0433, \u0436\u0438\u0440\u044b 24,0 \u0433, \u0443\u0433\u043b\u0435\u0432\u043e\u0434\u044b 43,0 \u0433.\u041a\u0430\u043b\u043e\u0440\u0438\u0439\u043d\u043e\u0441\u0442\u044c: 420 \u043a\u043a\u0430\u043b.\u0423\u0441\u043b\u043e\u0432\u0438\u044f \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f: \u043f\u0440\u0438 \u0442\u0435\u043c\u043f\u0435\u0440\u0430\u0442\u0443\u0440\u0435 -18\u00b0\u0421.\u0421\u0440\u043e\u043a \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f: 1 \u043c\u0435\u0441\u044f\u0446.\u0411\u0440\u0435\u043d\u0434: Rioba.\u041f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c: \u041c\u0435\u0442\u0440\u043e.\u0421\u0442\u0440\u0430\u043d\u0430-\u043f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c: \u0413\u0435\u0440\u043c\u0430\u043d\u0438\u044f.","5":"\u0421\u043c\u0435\u0441\u044c 5 \u043f\u0435\u0440\u0446\u0435\u0432 METRO CHEF, 140 \u0433 \u2014 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u043d\u0430\u044f \u0441\u043c\u0435\u0441\u044c \u043f\u0440\u044f\u043d\u043e\u0441\u0442\u0435\u0439, \u0441\u043e\u0441\u0442\u043e\u044f\u0449\u0430\u044f \u0438\u0437 \u043d\u0435\u0441\u043a\u043e\u043b\u044c\u043a\u0438\u0445 \u0440\u0430\u0437\u043d\u043e\u0432\u0438\u0434\u043d\u043e\u0441\u0442\u0435\u0439 \u043f\u0435\u0440\u0446\u0430. \u041a\u0430\u0436\u0434\u044b\u0439 \u0438\u0437 \u043a\u043e\u043c\u043f\u043e\u043d\u0435\u043d\u0442\u043e\u0432 \u0438\u043c\u0435\u0435\u0442 \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u044b\u0439 \u0432\u043a\u0443\u0441 \u0438 \u0430\u0440\u043e\u043c\u0430\u0442, \u0430 \u0432\u043c\u0435\u0441\u0442\u0435 \u043e\u043d\u0438 \u0441\u043e\u0437\u0434\u0430\u044e\u0442 \u043d\u0435\u043f\u043e\u0432\u0442\u043e\u0440\u0438\u043c\u043e\u0435 \u0441\u043e\u0447\u0435\u0442\u0430\u043d\u0438\u0435. \u0422\u0430\u043a\u0430\u044f \u0441\u043c\u0435\u0441\u044c \u043e\u0442\u043b\u0438\u0447\u0430\u0435\u0442\u0441\u044f \u0431\u043e\u0433\u0430\u0442\u044b\u043c \u0441\u043e\u0441\u0442\u0430\u0432\u043e\u043c, \u0432\u043a\u043b\u044e\u0447\u0430\u044e\u0449\u0438\u043c \u0432 \u0441\u0435\u0431\u044f \u0431\u043e\u043b\u044c\u0448\u043e\u0435 \u043a\u043e\u043b\u0438\u0447\u0435\u0441\u0442\u0432\u043e \u043f\u043e\u043b\u0435\u0437\u043d\u044b\u0445 \u0432\u0435\u0449\u0435\u0441\u0442\u0432: \u0432\u0438\u0442\u0430\u043c\u0438\u043d\u044b \u0410, \u0412, \u0421, \u0415, \u041a, \u0420\u0420, \u0430 \u0442\u0430\u043a\u0436\u0435 \u043c\u0435\u0434\u044c, \u0436\u0435\u043b\u0435\u0437\u043e, \u0444\u043e\u0441\u0444\u043e\u0440, \u0446\u0438\u043d\u043a, \u043a\u0430\u043b\u044c\u0446\u0438\u0439, \u043a\u0430\u043b\u0438\u0439 \u0438 \u0434\u0440\u0443\u0433\u0438\u0435 \u043c\u0438\u043d\u0435\u0440\u0430\u043b\u044b. \u0422\u0430\u043a\u0436\u0435 \u0432 \u043f\u0440\u0438\u043f\u0440\u0430\u0432\u0430\u0445 \u043f\u0440\u0438\u0441\u0443\u0442\u0441\u0442\u0432\u0443\u044e\u0442 \u044d\u0444\u0438\u0440\u043d\u044b\u0435 \u043c\u0430\u0441\u043b\u0430, \u043a\u043e\u0442\u043e\u0440\u044b\u0435 \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0441\u043e\u0437\u0434\u0430\u044e\u0442 \u0441\u0442\u043e\u0439\u043a\u0438\u0439 \u0430\u0440\u043e\u043c\u0430\u0442, \u043d\u043e \u0438 \u043f\u043e\u043b\u043e\u0436\u0438\u0442\u0435\u043b\u044c\u043d\u043e \u0432\u043b\u0438\u044f\u044e\u0442 \u043d\u0430 \u043d\u0435\u0440\u0432\u043d\u0443\u044e \u0438 \u043f\u0438\u0449\u0435\u0432\u0430\u0440\u0438\u0442\u0435\u043b\u044c\u043d\u0443\u044e \u0441\u0438\u0441\u0442\u0435\u043c\u044b. \u041f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u043d\u0430\u044f \u043f\u0440\u0438\u043f\u0440\u0430\u0432\u0430 \u0443\u043d\u0438\u0432\u0435\u0440\u0441\u0430\u043b\u044c\u043d\u0430 \u2014 \u0435\u0435 \u043c\u043e\u0436\u043d\u043e \u0434\u043e\u0431\u0430\u0432\u043b\u044f\u0442\u044c \u0432 \u0441\u0443\u043f\u044b \u0438 \u0431\u043e\u0440\u0449\u0438, \u0431\u043b\u044e\u0434\u0430 \u0438\u0437 \u043c\u044f\u0441\u0430, \u0440\u044b\u0431\u044b \u0438 \u043f\u0442\u0438\u0446\u044b, \u043a\u0430\u0440\u0442\u043e\u0444\u0435\u043b\u044c, \u043a\u0440\u0443\u043f\u044b, \u043c\u0430\u043a\u0430\u0440\u043e\u043d\u043d\u044b\u0435 \u0438\u0437\u0434\u0435\u043b\u0438\u044f, \u0431\u043e\u0431\u044b, \u0437\u0430\u043a\u0443\u0441\u043a\u0438, \u0441\u0430\u043b\u0430\u0442\u044b \u0438 \u0434\u0430\u0436\u0435 \u0434\u0435\u0441\u0435\u0440\u0442\u044b. \u041e\u043d\u0430 \u0433\u0430\u0440\u043c\u043e\u043d\u0438\u0447\u043d\u043e \u0441\u043e\u0447\u0435\u0442\u0430\u0435\u0442\u0441\u044f \u0441 \u0432\u043a\u0443\u0441\u043e\u043c \u043e\u0441\u043d\u043e\u0432\u043d\u044b\u0445 \u043f\u0440\u043e\u0434\u0443\u043a\u0442\u043e\u0432 \u0438 \u0434\u0430\u0440\u0438\u0442 \u0438\u043c \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u044b\u0439 \u0430\u0440\u043e\u043c\u0430\u0442. \u0421\u043c\u0435\u0441\u044c 5 \u043f\u0435\u0440\u0446\u0435\u0432 \u043c\u043e\u0436\u043d\u043e \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u0442\u044c \u0438 \u0432 \u043f\u0440\u0438\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u0438\u0438 \u0441\u043e\u0443\u0441\u043e\u0432 \u0438 \u043c\u0430\u0440\u0438\u043d\u0430\u0434\u043e\u0432. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u0441\u043c\u0435\u0441\u044c 5 \u043f\u0435\u0440\u0446\u0435\u0432 METRO CHEF, 140 \u0433 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u0412\u044b\u043f\u0443\u0441\u043a\u0430\u044f \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044e \u043f\u043e\u0434 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c\u0438 \u0442\u043e\u0440\u0433\u043e\u0432\u044b\u043c\u0438 \u043c\u0430\u0440\u043a\u0430\u043c\u0438 METRO, \u043c\u044b \u0441\u0442\u0440\u0435\u043c\u0438\u043c\u0441\u044f \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u0443\u0434\u043e\u0432\u043b\u0435\u0442\u0432\u043e\u0440\u0438\u0442\u044c \u043f\u043e\u0442\u0440\u0435\u0431\u043d\u043e\u0441\u0442\u0438 \u043d\u0430\u0448\u0438\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432. \u041d\u0430\u0448\u0430 \u0446\u0435\u043b\u044c \u2013 \u043f\u043e\u043d\u044f\u0442\u044c, \u043a\u0430\u043a\u043e\u0439 \u0430\u0441\u0441\u043e\u0440\u0442\u0438\u043c\u0435\u043d\u0442 \u0434\u043b\u044f \u043d\u0438\u0445 \u0430\u043a\u0442\u0443\u0430\u043b\u0435\u043d \u0438 \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0438\u0442\u044c \u0435\u0433\u043e \u043f\u043e \u043f\u0440\u0438\u0432\u043b\u0435\u043a\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u0446\u0435\u043d\u0435. \u041c\u044b \u0442\u0430\u043a\u0436\u0435 \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0438\u0437\u0443\u0447\u0430\u0435\u043c \u0442\u0435\u043d\u0434\u0435\u043d\u0446\u0438\u0438 \u0440\u044b\u043d\u043a\u0430, \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u0442\u044c \u0432 \u043a\u0443\u0440\u0441\u0435 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u0442\u0440\u0435\u043d\u0434\u043e\u0432 \u0438 \u0441\u0432\u043e\u0435\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0442\u044c \u0440\u0435\u0448\u0435\u043d\u0438\u044f \u043f\u043e\u0434 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c\u0438 \u0442\u043e\u0440\u0433\u043e\u0432\u044b\u043c\u0438 \u043c\u0430\u0440\u043a\u0430\u043c\u0438 \u0432 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0438 \u0441 \u043d\u0438\u043c\u0438. \u0411\u0440\u0435\u043d\u0434 METRO Chef \u0445\u043e\u0440\u043e\u0448\u043e \u0438\u0437\u0432\u0435\u0441\u0442\u0435\u043d \u0432\u043e \u0432\u0441\u0435\u043c \u043c\u0438\u0440\u0435. \u041f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044f \u044d\u0442\u043e\u0439 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u0439 \u043c\u0430\u0440\u043a\u0438 \u043e\u0442\u043b\u0438\u0447\u0430\u0435\u0442\u0441\u044f \u043e\u0442\u043c\u0435\u043d\u043d\u044b\u043c \u0432\u043a\u0443\u0441\u043e\u043c \u0438 \u043f\u0440\u0435\u0432\u043e\u0441\u0445\u043e\u0434\u043d\u044b\u043c \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u043e\u043c.","6":"","7":"\u0410\u0440\u0430\u0445\u0438\u0441 ARO \u0436\u0430\u0440\u0435\u043d\u044b\u0439 \u0441\u043e\u043b\u0435\u043d\u044b\u0439 \u2014 \u0441\u044b\u0442\u043d\u0430\u044f \u0438 \u0430\u0440\u043e\u043c\u0430\u0442\u043d\u0430\u044f \u0437\u0430\u043a\u0443\u0441\u043a\u0430 \u0432 \u0431\u043e\u043b\u044c\u0448\u043e\u0439 \u043f\u0430\u0447\u043a\u0435, \u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u0445\u0432\u0430\u0442\u0438\u0442 \u043d\u0430 \u0431\u043e\u043b\u044c\u0448\u0443\u044e \u043a\u043e\u043c\u043f\u0430\u043d\u0438\u044e. \u041d\u0430\u0442\u0443\u0440\u0430\u043b\u044c\u043d\u044b\u0439 \u0432\u044b\u0441\u043e\u043a\u043e\u043a\u0430\u043b\u043e\u0440\u0438\u0439\u043d\u044b\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0442 \u0441\u0442\u0430\u043d\u0435\u0442 \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u043c \u0434\u043e\u043f\u043e\u043b\u043d\u0435\u043d\u0438\u0435\u043c \u043a \u0440\u0430\u0437\u043b\u0438\u0447\u043d\u044b\u043c \u043d\u0430\u043f\u0438\u0442\u043a\u0430\u043c, \u0430 \u0442\u0430\u043a\u0436\u0435 \u043c\u043e\u0436\u0435\u0442 \u0432\u044b\u0441\u0442\u0443\u043f\u0438\u0442\u044c \u0432 \u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0441\u0430\u043c\u043e\u0441\u0442\u043e\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0433\u043e \u043f\u0435\u0440\u0435\u043a\u0443\u0441\u0430. \u041d\u0430 \u043f\u0440\u043e\u0433\u0443\u043b\u043a\u0435 \u0438\u043b\u0438 \u0432 \u0434\u043e\u0440\u043e\u0433\u0435, \u0432\u043e \u0432\u0440\u0435\u043c\u044f \u043f\u0440\u043e\u0441\u043c\u043e\u0442\u0440\u0430 \u0444\u0438\u043b\u044c\u043c\u0430 \u0438\u043b\u0438 \u0444\u0443\u0442\u0431\u043e\u043b\u044c\u043d\u043e\u0433\u043e \u043c\u0430\u0442\u0447\u0430 \u2014 \u043f\u0438\u0442\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0435 \u043e\u0440\u0435\u0448\u043a\u0438 \u0443\u0442\u043e\u043b\u044f\u0442 \u0433\u043e\u043b\u043e\u0434 \u0438 \u043f\u043e\u0434\u043d\u0438\u043c\u0443\u0442 \u043d\u0430\u0441\u0442\u0440\u043e\u0435\u043d\u0438\u0435! \u041e\u0431\u0436\u0430\u0440\u0435\u043d\u043d\u044b\u0435 \u044f\u0434\u0440\u0430 \u043e\u0440\u0435\u0448\u043a\u043e\u0432 \u043d\u0435 \u0442\u043e\u043b\u044c\u043a\u043e \u0432\u043a\u0443\u0441\u043d\u044b, \u043d\u043e \u0438 \u043f\u043e\u043b\u0435\u0437\u043d\u044b. \u041f\u0440\u0438 \u043d\u0430\u0433\u0440\u0435\u0432\u0430\u043d\u0438\u0438 \u0432 \u043d\u0438\u0445 \u043f\u043e\u0432\u044b\u0448\u0430\u0435\u0442\u0441\u044f \u0443\u0440\u043e\u0432\u0435\u043d\u044c \u043f\u043e\u043b\u0438\u0444\u0435\u043d\u043e\u043b\u0430, \u043a\u043e\u0442\u043e\u0440\u044b\u0439 \u0432\u0445\u043e\u0434\u0438\u0442 \u0432 \u0447\u0438\u0441\u043b\u043e \u043c\u043e\u0449\u043d\u044b\u0445 \u043f\u0440\u0438\u0440\u043e\u0434\u043d\u044b\u0445 \u0430\u043d\u0442\u0438\u043e\u043a\u0441\u0438\u0434\u0430\u043d\u0442\u043e\u0432: \u0432 \u043e\u0431\u0440\u0430\u0431\u043e\u0442\u0430\u043d\u043d\u043e\u043c \u0430\u0440\u0430\u0445\u0438\u0441\u0435 \u0438\u0445 \u0441\u0442\u043e\u043b\u044c\u043a\u043e \u0436\u0435, \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0432 \u043c\u0430\u043b\u0438\u043d\u0435. \u041a\u0443\u043f\u0438\u0442\u044c \u0430\u0440\u0430\u0445\u0438\u0441 ARO \u0436\u0430\u0440\u0435\u043d\u044b\u0439 \u0441\u043e\u043b\u0435\u043d\u044b\u0439 \u0432\u044b \u043c\u043e\u0436\u0435\u0442\u0435 \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u041f\u0440\u043e\u0438\u0437\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044c: Aro, \u0420\u043e\u0441\u0441\u0438\u044f.\u0421\u043e\u0441\u0442\u0430\u0432: \u044f\u0434\u0440\u0430 \u043e\u0440\u0435\u0445\u043e\u0432 \u0430\u0440\u0430\u0445\u0438\u0441\u0430 \u0436\u0430\u0440\u0435\u043d\u044b\u0435, \u0441\u043e\u043b\u044c.\u041f\u0438\u0449\u0435\u0432\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: \u0431\u0435\u043b\u043a\u0438 \u2014 28,3 \u0433, \u0436\u0438\u0440\u044b \u2014 48,7 \u0433, \u0443\u0433\u043b\u0435\u0432\u043e\u0434\u044b \u2014 9,9 \u0433.\u042d\u043d\u0435\u0440\u0433\u0435\u0442\u0438\u0447\u0435\u0441\u043a\u0430\u044f \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u044c: 613 \u043a\u043a\u0430\u043b.\u0421\u0440\u043e\u043a \u0433\u043e\u0434\u043d\u043e\u0441\u0442\u0438: 6 \u043c\u0435\u0441\u044f\u0446\u0435\u0432.","8":"\u0427\u0438\u043f\u0441\u044b \u043a\u0443\u043a\u0443\u0440\u0443\u0437\u043d\u044b\u0435 BBQ METRO CHEF \u043f\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u044e\u0442\u0441\u044f \u043e\u0431\u044a\u0435\u043c\u043e\u043c 750 \u0433. \u041e\u043d\u0438 \u043e\u0442\u043b\u0438\u0447\u043d\u043e \u0443\u0442\u043e\u043b\u044f\u044e\u0442 \u0447\u0443\u0432\u0441\u0442\u0432\u043e \u0433\u043e\u043b\u043e\u0434\u0430, \u043e\u0441\u043e\u0431\u0435\u043d\u043d\u043e \u0432 \u043f\u0435\u0440\u0435\u0440\u044b\u0432\u0430\u0445 \u043c\u0435\u0436\u0434\u0443 \u043f\u0440\u0438\u0435\u043c\u0430\u043c\u0438 \u043f\u0438\u0449\u0438. \u0427\u0438\u043f\u0441\u044b \u0433\u043e\u0442\u043e\u0432\u044f\u0442 \u0438\u0437 \u043a\u0443\u043a\u0443\u0440\u0443\u0437\u043d\u043e\u0439 \u043c\u0443\u043a\u0438 \u0432\u044b\u0441\u0448\u0435\u0433\u043e \u0441\u043e\u0440\u0442\u0430, \u043e\u043d\u0438 \u043c\u0435\u043d\u0435\u0435 \u043a\u0430\u043b\u043e\u0440\u0438\u0439\u043d\u044b, \u0432 \u043e\u0442\u043b\u0438\u0447\u0438\u0435 \u043e\u0442 \u0447\u0438\u043f\u0441\u043e\u0432, \u0441\u0434\u0435\u043b\u0430\u043d\u043d\u044b\u0445 \u0438\u0437 \u043a\u0430\u0440\u0442\u043e\u0444\u0435\u043b\u044f. \u0413\u043e\u0442\u043e\u0432\u044b\u0439 \u043f\u0440\u043e\u0434\u0443\u043a\u0442 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 \u0441\u043e\u0431\u043e\u0439 \u0447\u0438\u043f\u0441\u044b \u0437\u043e\u043b\u043e\u0442\u0438\u0441\u0442\u043e\u0433\u043e \u0446\u0432\u0435\u0442\u0430 \u0441 \u0445\u0440\u0443\u0441\u0442\u044f\u0449\u0435\u0439 \u0442\u0435\u043a\u0441\u0442\u0443\u0440\u043e\u0439. \u041e\u043d\u0438 \u043e\u0442\u043b\u0438\u0447\u0430\u044e\u0442\u0441\u044f \u043f\u0440\u0438\u044f\u0442\u043d\u044b\u043c \u0432\u043a\u0443\u0441\u043e\u043c \u0438 \u0442\u043e\u043d\u043a\u0438\u043c \u043f\u0438\u043a\u0430\u043d\u0442\u043d\u044b\u043c \u0430\u0440\u043e\u043c\u0430\u0442\u043e\u043c. \u0427\u0438\u043f\u0441\u044b \u0443\u043f\u0430\u043a\u043e\u0432\u0430\u043d\u044b \u0432 \u0444\u043e\u043b\u044c\u0433\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u0443\u044e \u043f\u0430\u0447\u043a\u0443, \u0443\u0434\u043e\u0431\u043d\u0443\u044e \u0434\u043b\u044f \u0438\u0441\u043f\u043e\u043b\u044c\u0437\u043e\u0432\u0430\u043d\u0438\u044f \u0438 \u043f\u0435\u0440\u0435\u043d\u043e\u0441\u043a\u0438. \u0412\u043e\u0442 \u043f\u043e\u0447\u0435\u043c\u0443 \u0438\u0445 \u043c\u043e\u0436\u043d\u043e \u0431\u0440\u0430\u0442\u044c \u0441 \u0441\u043e\u0431\u043e\u0439 \u0432 \u043f\u043e\u0435\u0437\u0434\u043a\u0438, \u043d\u0430 \u043f\u0440\u043e\u0433\u0443\u043b\u043a\u0443, \u0432 \u043f\u0443\u0442\u0435\u0448\u0435\u0441\u0442\u0432\u0438\u044f. \u0421\u043d\u0435\u043a\u0438 \u0443\u043f\u043e\u0442\u0440\u0435\u0431\u043b\u044f\u044e\u0442 \u0441 \u0433\u0430\u0437\u0438\u0440\u043e\u0432\u0430\u043d\u043d\u043e\u0439 \u0432\u043e\u0434\u043e\u0439 \u0438\u043b\u0438 \u043f\u0438\u0432\u043e\u043c, \u0430 \u0442\u0430\u043a\u0436\u0435 \u043d\u0435\u043a\u043e\u0442\u043e\u0440\u044b\u043c\u0438 \u0432\u0438\u0434\u0430\u043c\u0438 \u0441\u043e\u0443\u0441\u043e\u0432. \u041f\u0440\u0438\u043e\u0431\u0440\u0435\u0441\u0442\u0438 \u0447\u0438\u043f\u0441\u044b \u043a\u0443\u043a\u0443\u0440\u0443\u0437\u043d\u044b\u0435 BBQ METRO CHEF, 750 \u0433 \u043c\u043e\u0436\u043d\u043e \u0432 \u0421\u0430\u043d\u043a\u0442-\u041f\u0435\u0442\u0435\u0440\u0431\u0443\u0440\u0433\u0435 \u0432 \u0442\u043e\u0440\u0433\u043e\u0432\u043e\u043c \u0446\u0435\u043d\u0442\u0440\u0435 METRO.\u0412\u044b\u043f\u0443\u0441\u043a\u0430\u044f \u043f\u0440\u043e\u0434\u0443\u043a\u0446\u0438\u044e \u043f\u043e\u0434 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c\u0438 \u0442\u043e\u0440\u0433\u043e\u0432\u044b\u043c\u0438 \u043c\u0430\u0440\u043a\u0430\u043c\u0438 METRO, \u043c\u044b \u0441\u0442\u0440\u0435\u043c\u0438\u043c\u0441\u044f \u043c\u0430\u043a\u0441\u0438\u043c\u0430\u043b\u044c\u043d\u043e \u0443\u0434\u043e\u0432\u043b\u0435\u0442\u0432\u043e\u0440\u0438\u0442\u044c \u043f\u043e\u0442\u0440\u0435\u0431\u043d\u043e\u0441\u0442\u0438 \u043d\u0430\u0448\u0438\u0445 \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432. \u041d\u0430\u0448\u0430 \u0446\u0435\u043b\u044c - \u043f\u043e\u043d\u044f\u0442\u044c, \u043a\u0430\u043a\u043e\u0439 \u0430\u0441\u0441\u043e\u0440\u0442\u0438\u043c\u0435\u043d\u0442 \u0434\u043b\u044f \u043d\u0438\u0445 \u0430\u043a\u0442\u0443\u0430\u043b\u0435\u043d \u0438 \u043f\u0440\u0435\u0434\u043b\u043e\u0436\u0438\u0442\u044c \u0435\u0433\u043e \u043f\u043e \u043f\u0440\u0438\u0432\u043b\u0435\u043a\u0430\u0442\u0435\u043b\u044c\u043d\u043e\u0439 \u0446\u0435\u043d\u0435. \u041c\u044b \u0442\u0430\u043a\u0436\u0435 \u0432\u043d\u0438\u043c\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u0438\u0437\u0443\u0447\u0430\u0435\u043c \u0442\u0435\u043d\u0434\u0435\u043d\u0446\u0438\u0438 \u0440\u044b\u043d\u043a\u0430, \u0447\u0442\u043e\u0431\u044b \u0431\u044b\u0442\u044c \u0432 \u043a\u0443\u0440\u0441\u0435 \u043f\u043e\u0441\u043b\u0435\u0434\u043d\u0438\u0445 \u0442\u0440\u0435\u043d\u0434\u043e\u0432 \u0438 \u0441\u0432\u043e\u0435\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u043e \u043f\u0440\u0435\u0434\u043b\u0430\u0433\u0430\u0442\u044c \u0440\u0435\u0448\u0435\u043d\u0438\u044f \u043f\u043e\u0434 \u0441\u043e\u0431\u0441\u0442\u0432\u0435\u043d\u043d\u044b\u043c\u0438 \u0442\u043e\u0440\u0433\u043e\u0432\u044b\u043c\u0438 \u043c\u0430\u0440\u043a\u0430\u043c\u0438 \u0432 \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0438\u0438